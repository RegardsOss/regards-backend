			<xs:attribute name="GRAPH_TYPE" use="required" fixed="DOCUMENT"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="DOCUMENT_COLLECTION_UPDATE">
		<xs:annotation>
			<xs:documentation>Description d'une collection de documents</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="NODE_IDENTIFIER"/>
