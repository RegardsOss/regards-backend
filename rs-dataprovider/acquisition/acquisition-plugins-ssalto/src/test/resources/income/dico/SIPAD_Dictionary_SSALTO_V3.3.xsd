			<xs:attribute name="ENTITY_TYPE" use="required" fixed="DOCUMENT_DESCRIPTION"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="DOCUMENT_UPDATE">
		<xs:annotation>
