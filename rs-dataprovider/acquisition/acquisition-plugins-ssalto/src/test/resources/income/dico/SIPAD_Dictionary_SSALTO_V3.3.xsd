	<xs:element name="MASTER_DESCRIPTION">
		<xs:annotation>
			<xs:documentation>permet d'ajouter un master</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="MASTER">
					<xs:annotation>
						<xs:documentation>description d'un master</xs:documentation>
					</xs:annotation>
					<xs:complexType>
						<xs:sequence>
							<xs:element ref="LABEL"/>
							<xs:element name="PLUGIN_LOCATION" type="xs:string">
								<xs:annotation>
									<xs:documentation>Emplacement de l executable plugin</xs:documentation>
								</xs:annotation>
							</xs:element>
						</xs:sequence>
					</xs:complexType>
				</xs:element>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:element name="COLLECTION_DESCRIPTION">
		<xs:annotation>
			<xs:documentation>Description d'une collection du graphe de donnees ou browse ou table d'evenements</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="NODE_IDENTIFIER"/>
				<xs:element ref="GRAPH_ROOT"/>
				<xs:element ref="LABEL"/>
				<xs:element ref="SHORT_DESCRIPTION" minOccurs="0"/>
				<xs:element ref="LONG_DESCRIPTION" minOccurs="0"/>
				<xs:element ref="EVENT_TABLE_IDENTIFIER" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attribute name="ENTITY_TYPE" use="required" fixed="COLLECTION_DESCRIPTION"/>
			<xs:attribute name="GRAPH_TYPE" type="TypeGraphType" use="required"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="COLLECTION_UPDATE">
		<xs:annotation>
			<xs:documentation>Modification d'une collection du graphe de donnees ou browse ou table d'evenements</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="NODE_IDENTIFIER"/>
				<xs:element ref="LABEL" minOccurs="0"/>
				<xs:element ref="SHORT_DESCRIPTION" minOccurs="0"/>
				<xs:element ref="LONG_DESCRIPTION" minOccurs="0"/>
				<xs:element ref="EVENT_TABLE_IDENTIFIER" minOccurs="0" maxOccurs="unbounded"/>
			</xs:sequence>
			<xs:attribute name="ENTITY_TYPE" use="required" fixed="COLLECTION_UPDATE"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="DATA_STORAGE_OBJECT_DESCRIPTION">
		<xs:annotation>
			<xs:documentation>Description d'un objet de stockage de donnees</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element ref="DATA_STORAGE_OBJECT_IDENTIFIER"/>
				<xs:element ref="FILE_SIZE"/>
				<xs:element ref="STORAGE"/>
				<xs:element ref="TRANSFORMATION_SO_DO" minOccurs="0"/>
			</xs:sequence>
			<xs:attribute name="ENTITY_TYPE" use="required" fixed="DATA_STORAGE_OBJECT_DESCRIPTION"/>
		</xs:complexType>
	</xs:element>
	<xs:element name="DATA_STORAGE_OBJECT_UPDATE">
