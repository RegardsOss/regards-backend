	<xs:complexType name="TypeCBothStorage">
		<xs:annotation>
			<xs:documentation>Stockage en archive et en ligne</xs:documentation>
		</xs:annotation>
		<xs:all>
			<xs:element name="STORAGE_ON_LINE" type="TypeCStorageOnLine">
				<xs:annotation>
					<xs:documentation>Objet en ligne</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="STORAGE_NEAR_LINE" type="TypeCStorageNearLine">
				<xs:annotation>
					<xs:documentation>Objet dans l'archive</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:all>
	</xs:complexType>
	<xs:complexType name="TypeCBothStorageUpdate">
		<xs:annotation>
			<xs:documentation>Stockage en archive et en ligne</xs:documentation>
		</xs:annotation>
		<xs:all>
			<xs:element name="STORAGE_ON_LINE" type="TypeCStorageOnLineUpdate">
				<xs:annotation>
					<xs:documentation>Objet en ligne</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="STORAGE_NEAR_LINE" type="TypeCStorageNearLineUpdate">
				<xs:annotation>
					<xs:documentation>Objet dans l'archive</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:all>
	</xs:complexType>
