    <xs:simpleType name="TypeArchive">
        <xs:annotation>
            <xs:documentation>Enumere des noms d'archives possibles pour un objet de stockage</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="SGDS"/>
            <xs:enumeration value="GF_SGDS"/>
            <xs:enumeration value="AVISO_COMMUN"/>
            <xs:enumeration value="AVISO_CRYOSAT"/>
            <xs:enumeration value="AVISO_ENVISAT"/>
            <xs:enumeration value="AVISO_ERS1"/>
            <xs:enumeration value="AVISO_ERS2"/>
            <xs:enumeration value="AVISO_JASON2"/>
            <xs:enumeration value="AVISO_JASON"/>
            <xs:enumeration value="AVISO_SPOT"/>
            <xs:enumeration value="AVISO_TP"/>
