    </xs:simpleType>
    <xs:simpleType name="TypeEventTableIdentifier">
        <xs:annotation>
            <xs:documentation>String TypeString_1_128 debutant par TAB_TC_... </xs:documentation>
        </xs:annotation>
        <xs:restriction base="TypeString_1_128">
            <xs:pattern value="TAB_TC_.+"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="TypeEventTableValueType">
        <xs:annotation>
