            <xs:documentation>String TypeString_1_80 de la forme  &lt;&amp;...&gt;</xs:documentation>
        </xs:annotation>
        <xs:restriction base="TypeString_1_80">
            <xs:minLength value="1"/>
            <xs:pattern value="&lt;&amp;.*&gt;"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="TypeTransfoSoDo">
        <xs:annotation>
            <xs:documentation>Enumere des transformations pour passer d'un objet de stockage a un objet de donnees</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="CAT"/>
            <xs:enumeration value="DETAR"/>
            <xs:enumeration value="OD_DETAR"/>
            <xs:enumeration value="GUNZIP"/>
            <xs:enumeration value="IDENTITY"/>
            <xs:enumeration value="UNZIP"/>
            <xs:enumeration value="CLONE_CATAVISO"/>
        </xs:restriction>
    </xs:simpleType>
