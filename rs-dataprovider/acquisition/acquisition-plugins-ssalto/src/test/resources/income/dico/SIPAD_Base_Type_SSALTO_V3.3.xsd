            <xs:enumeration value="SSALTO_SPOT_HY2_SARAL"/>
            <xs:enumeration value="SSALTO_TOPEX_JASON_COMMUN"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="TypeArchivePath">
        <xs:annotation>
            <xs:documentation>String de 1 a 160 car. debutant par un /</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="1"/>
            <xs:maxLength value="160"/>
            <xs:pattern value="/.{1,159}"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="TypeDocumentPriority">
        <xs:annotation>
            <xs:documentation>Enumere des ordres d'importance des documents</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:enumeration value="PRIMARY"/>
            <xs:enumeration value="SECONDARY"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="TypeDocumentType">
