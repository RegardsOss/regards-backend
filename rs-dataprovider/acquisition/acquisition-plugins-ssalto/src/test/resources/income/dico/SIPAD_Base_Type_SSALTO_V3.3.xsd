    <xs:simpleType name="TypeIssueRevision">
        <xs:annotation>
            <xs:documentation>String de 15 a 17 car. de format "Issue n  - Rev n" avec n = entier a 1 ou 2 digits</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:string">
            <xs:minLength value="15"/>
            <xs:maxLength value="17"/>
            <xs:pattern value="Issue [0-9][0-9]? - Rev [0-9A-C][0-9A-C]?"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="TypeLatitude">
        <xs:annotation>
            <xs:documentation>Decimal de format (nn)n.dd [-90.00; 90.00]</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:decimal">
            <xs:fractionDigits value="2"/>
            <xs:minInclusive value="-90"/>
            <xs:maxInclusive value="90"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="TypeLongitude">
        <xs:annotation>
            <xs:documentation>Decimal de format (nnn)n.dd [-180.00; 180.00]</xs:documentation>
        </xs:annotation>
        <xs:restriction base="xs:decimal">
            <xs:maxInclusive value="180"/>
            <xs:fractionDigits value="2"/>
            <xs:minInclusive value="-180"/>
        </xs:restriction>
    </xs:simpleType>
