            <xs:enumeration value="BIBLIOGRAPHIE"/>
            <xs:enumeration value="DED"/>
            <xs:enumeration value="DOC"/>
            <xs:enumeration value="LOGICIEL"/>
        </xs:restriction>
    </xs:simpleType>
    <xs:simpleType name="TypeEventTableCriteriaType">
        <xs:annotation>
            <xs:documentation>Enumere des types possibles de criteres renseignes dans une table d'evenements</xs:documentation>
