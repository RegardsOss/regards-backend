<PLUGIN_CONFIGURATION xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="schemas_plugins.xsd">
    <fileNamePattern>(SRL_XNG_OP)([a-zA-Z]{1})(7([0-9]{3}))</fileNamePattern>
    <compositeAttribute>
        <name>TIME_PERIOD</name>
        <order>1</order>
        <attributeFromAsciiFile>
            <linePattern>First_Measurement_Time_Of_Mission_1 =  ([0-9T:\-]*)\..*</linePattern>
            <lineNumber>17</lineNumber>
            <groupNumber>1</groupNumber>
            <specification>
                <name>START_DATE</name>
                <valueType>DATE_TIME</valueType>
                <formatRead>yyyy-MM-dd'T'HH:mm:ss</formatRead>
            </specification>
        </attributeFromAsciiFile>
        <attributeFromAsciiFile>
            <linePattern>Last_Measurement_Time_Of_Mission_1 =  ([0-9T:\-]*)\..*</linePattern>
            <lineNumber>18</lineNumber>
            <groupNumber>1</groupNumber>
            <specification>
                <name>STOP_DATE</name>
                <valueType>DATE_TIME</valueType>
                <formatRead>yyyy-MM-dd'T'HH:mm:ss</formatRead>
            </specification>
        </attributeFromAsciiFile>
    </compositeAttribute>
    <attributeFromName>
        <groupNumber>1</groupNumber>
        <groupNumber>3</groupNumber>
        <specification>
            <name>RADICAL</name>
            <valueType>STRING</valueType>
            <order>5</order>
        </specification>
    </attributeFromName>
    <attributeFromName>
        <groupNumber>2</groupNumber>
        <specification>
            <name>OBJECT_VERSION</name>
            <valueType>STRING</valueType>
            <order>6</order>
        </specification>
    </attributeFromName>
    <attributeFromAsciiFile>
        <linePattern>Product_Creation_Time = ([0-9T:\-]*)\..*</linePattern>
        <lineNumber>9</lineNumber>
        <groupNumber>1</groupNumber>
        <specification>
            <name>FILE_CREATION_DATE</name>
            <valueType>DATE_TIME</valueType>
            <order>7</order>    
            <formatRead>yyyy-MM-dd'T'HH:mm:ss</formatRead>
        </specification>
    </attributeFromAsciiFile>
</PLUGIN_CONFIGURATION>
