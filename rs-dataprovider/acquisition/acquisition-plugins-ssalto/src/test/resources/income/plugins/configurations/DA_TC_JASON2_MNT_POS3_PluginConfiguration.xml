<PLUGIN_CONFIGURATION xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="schemas_plugins.xsd">
	<fileNamePattern>(POS3_.*_MNT_)(.*)(\.)(.*)_([0-9]{4}_[0-9]{2}_[0-9]{2}_[0-9]{2}_[0-9]{2}_[0-9]{2})(_EEPROM_)(.*)(\.REF)</fileNamePattern>
	<compositeAttribute>
		<name>TIME_PERIOD</name>
		<order>1</order>
		<attributeFromName>
			<groupNumber>5</groupNumber>
			<specification>
				<name>START_DATE</name>
				<valueType>DATE_TIME</valueType>
				<formatRead>yyyy_MM_dd_HH_mm_ss</formatRead>
			</specification>
		</attributeFromName>
		<attributeFromOther>
			<otherAttributeName>START_DATE</otherAttributeName>
			<specification>
				<name>STOP_DATE</name>
				<valueType>DATE_TIME</valueType>
			</specification>
		</attributeFromOther>
	</compositeAttribute>
	<attributeFromName>
		<groupNumber>7</groupNumber>
		<specification>
			<name>PRODUCT_OPTION</name>
			<valueType>STRING</valueType>
			<order>3</order>
		</specification>
	</attributeFromName>
	<attributeFromName>
		<groupNumber>1</groupNumber>
		<groupNumber>5</groupNumber>
		<groupNumber>6</groupNumber>
		<groupNumber>7</groupNumber>
		<groupNumber>8</groupNumber>
		<specification>
			<name>RADICAL</name>
			<valueType>STRING</valueType>
			<order>4</order>
		</specification>
	</attributeFromName>
	<attributeFromName>
		<groupNumber>2</groupNumber>
		<groupNumber>3</groupNumber>
		<groupNumber>4</groupNumber>
		<specification>
			<name>OBJECT_VERSION</name>
			<valueType>STRING</valueType>
			<order>5</order>
		</specification>
	</attributeFromName>
	<fileInfoAttribute>
		<fileInformation>LAST_MODIFICATION_DATE</fileInformation>
		<specification>
			<name>FILE_CREATION_DATE</name>
			<valueType>DATE_TIME</valueType>
			<order>6</order>
		</specification>
	</fileInfoAttribute>
</PLUGIN_CONFIGURATION>
