<PLUGIN_CONFIGURATION xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="schemas_plugins.xsd">
	<fileNamePattern>(CS__POR_AX([VP]{1})CNE)(_){0,1}([0-9]{8}_[0-9]{6})((_){0,1}([0-9]{8}_[0-9]{6})(_){0,1}([0-9]{8}_[0-9]{6}))</fileNamePattern>
	<compositeAttribute>
		<name>TIME_PERIOD</name>
		<order>3</order>
		<attributeFromName>
			<groupNumber>7</groupNumber>
			<specification>
				<name>START_DATE</name>
				<valueType>DATE_TIME</valueType>
				<order>1</order>
				<formatRead>yyyyMMdd_HHmmss</formatRead>
			</specification>
		</attributeFromName>
		<attributeFromName>
			<groupNumber>9</groupNumber>
			<specification>
				<name>STOP_DATE</name>
				<valueType>DATE_TIME</valueType>
				<order>2</order>
				<formatRead>yyyyMMdd_HHmmss</formatRead>
			</specification>
		</attributeFromName>
	</compositeAttribute>
	<compositeAttribute>
		<name>CYCLE_RANGE</name>
		<order>6</order>
		<translatedAttributeFromCycleFile>
			<otherAttributeName>START_DATE</otherAttributeName>
			<specification>
				<name>CYCLE_MIN</name>
				<valueType>INTEGER</valueType>
				<order>4</order>
			</specification>
		</translatedAttributeFromCycleFile>
		<translatedAttributeFromCycleFile>
			<otherAttributeName>STOP_DATE</otherAttributeName>
			<specification>
				<name>CYCLE_MAX</name>
				<valueType>INTEGER</valueType>
				<order>5</order>
			</specification>
		</translatedAttributeFromCycleFile>
	</compositeAttribute>
	<translatedAttributeFromName>
		<groupNumber>2</groupNumber>
		<translationFilePath>/MOE_TranslationFile.properties</translationFilePath>
		<specification>
			<name>PRODUCT_OPTION</name>
			<valueType>STRING</valueType>
			<order>7</order>
		</specification>
	</translatedAttributeFromName>
	<attributeFromName>
		<groupNumber>1</groupNumber>
		<groupNumber>5</groupNumber>
		<specification>
			<name>RADICAL</name>
			<valueType>STRING</valueType>
			<order>8</order>
		</specification>
	</attributeFromName>
	<attributeFromName>
		<groupNumber>4</groupNumber>
		<specification>
			<name>FILE_CREATION_DATE</name>
			<valueType>DATE_TIME</valueType>
			<order>9</order>
			<formatRead>yyyyMMdd_HHmmss</formatRead>
		</specification>
	</attributeFromName>
</PLUGIN_CONFIGURATION>
