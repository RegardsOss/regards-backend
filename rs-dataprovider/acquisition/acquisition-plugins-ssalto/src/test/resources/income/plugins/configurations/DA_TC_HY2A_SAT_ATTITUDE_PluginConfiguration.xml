<PLUGIN_CONFIGURATION xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="schemas_plugins.xsd">
    <fileNamePattern>H2A_SA_([0-9]{8}_[0-9]{6})</fileNamePattern>
    <compositeAttribute>
        <name>TIME_PERIOD</name>
        <order>1</order>
        <attributeFromAsciiFile>
            <linePattern>
                ([0-9]{4}/[0-9]{2}/[0-9]{2} [0-9]{2}:[0-9]{2}:[0-9]{2}).*
            </linePattern>
            <lineNumber>6</lineNumber>
            <groupNumber>1</groupNumber>
            <specification>
                <name>START_DATE</name>
                <valueType>DATE_TIME</valueType>
                <formatRead>yyyy/MM/dd hh:mm:ss</formatRead>
            </specification>
        </attributeFromAsciiFile>
        <attributeFromAsciiFile>
            <linePattern>
                ([0-9]{4}/[0-9]{2}/[0-9]{2} [0-9]{2}:[0-9]{2}:[0-9]{2}).*
            </linePattern>
            <lineNumber>-2</lineNumber> 
            <groupNumber>1</groupNumber>
            <specification>
                <name>STOP_DATE</name>
                <valueType>DATE_TIME</valueType>
                <formatRead>yyyy/MM/dd hh:mm:ss</formatRead>
            </specification>
        </attributeFromAsciiFile>
    </compositeAttribute>

   <compositeAttribute>
        <name>CYCLE_RANGE</name>
        <order>4</order>
        <translatedAttributeFromCycleFile>
            <otherAttributeName>START_DATE</otherAttributeName>
            <specification>
                <name>CYCLE_MIN</name>
                <valueType>INTEGER</valueType>
            </specification>
        </translatedAttributeFromCycleFile>
        <translatedAttributeFromCycleFile>
            <otherAttributeName>STOP_DATE</otherAttributeName>
            <specification>
                <name>CYCLE_MAX</name>
                <valueType>INTEGER</valueType>
            </specification>
        </translatedAttributeFromCycleFile>
    </compositeAttribute>


    <attributeFromName>
        <groupNumber>1</groupNumber>
        <specification>
            <name>FILE_CREATION_DATE</name>
            <valueType>DATE_TIME</valueType>
            <order>2</order>
            <formatRead>yyyyMMdd_HHmmss</formatRead>
        </specification>
    </attributeFromName>

</PLUGIN_CONFIGURATION>
