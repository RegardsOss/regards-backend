<PLUGIN_CONFIGURATION xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="schemas_plugins.xsd">
    <fileNamePattern>(DS3A_MED_1P)([a-z]{1})(D([0-9]{8})_000000)</fileNamePattern>
    <compositeAttribute>
        <name>TIME_PERIOD</name>
        <order>1</order>
        <attributeFromName>
            <groupNumber>4</groupNumber>
            <specification>
                <name>START_DATE</name>
                <valueType>DATE_TIME</valueType>
                <formatRead>yyyyMMdd</formatRead>
            </specification>
        </attributeFromName>
    <attributeFromName>
            <groupNumber>4</groupNumber>
            <specification>
                <name>STOP_DATE</name>
                <valueType>DATE_TIME</valueType>
                <formatRead>yyyyMMdd</formatRead>
                <calculationClass>ssalto.domain.plugins.impl.calc.SetStopDateHours</calculationClass>
            </specification>
        </attributeFromName>
    </compositeAttribute>

    <compositeAttribute>
        <name>CYCLE_RANGE</name>
      <order>3</order>
        <translatedAttributeFromCycleFile>
            <otherAttributeName>START_DATE</otherAttributeName>
            <specification>
                <name>CYCLE_MIN</name>
                <valueType>INTEGER</valueType>
            </specification>
        </translatedAttributeFromCycleFile>
        <translatedAttributeFromCycleFile>
            <otherAttributeName>STOP_DATE</otherAttributeName>
            <specification>
                <name>CYCLE_MAX</name>
                <valueType>INTEGER</valueType>
            </specification>
        </translatedAttributeFromCycleFile>
    </compositeAttribute>

    <attributeFromName>
        <groupNumber>1</groupNumber>
        <groupNumber>3</groupNumber>
        <specification>
            <name>RADICAL</name>
            <valueType>STRING</valueType>
            <order>6</order>
        </specification>
    </attributeFromName>

    <fileInfoAttribute>
            <fileInformation>LAST_MODIFICATION_DATE</fileInformation>
            <specification>
                <name>FILE_CREATION_DATE</name>
                <valueType>DATE_TIME</valueType>
                <order>4</order>
            </specification>
    </fileInfoAttribute>

    <attributeFromName>
            <groupNumber>2</groupNumber>
            <specification>
                <name>OBJECT_VERSION</name>
                <valueType>STRING</valueType>
                <order>5</order>
            </specification>
     </attributeFromName>

</PLUGIN_CONFIGURATION>
