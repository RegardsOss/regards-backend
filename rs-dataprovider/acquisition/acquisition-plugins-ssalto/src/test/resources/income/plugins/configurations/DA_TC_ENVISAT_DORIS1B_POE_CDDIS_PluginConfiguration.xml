<PLUGIN_CONFIGURATION xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="schemas_plugins.xsd">
	<fileNamePattern>en1data([0-9]{3}).dat.Z</fileNamePattern>
	<compositeAttribute>
		<name>TIME_PERIOD</name>
		<order>1</order>
		<attributeFromName>
			<groupNumber>1</groupNumber>
			<specification>
				<name>START_DATE</name>
				<valueType>INTEGER</valueType>
				<calculationClass>fr.cnes.regards.modules.acquisition.plugins.ssalto.calc.SetEnvisatD1BPoeCddisStartDate</calculationClass>
			</specification>
		</attributeFromName>
        
        <attributeFromOther>
			<otherAttributeName>START_DATE</otherAttributeName>
			<specification>
				<name>STOP_DATE</name>
				<valueType>INTEGER</valueType>
				<calculationClass>fr.cnes.regards.modules.acquisition.plugins.ssalto.calc.SetEnvisatD1BPoeCddisStopDate</calculationClass>
			</specification>
		</attributeFromOther>
		
	</compositeAttribute>

	<compositeAttribute>
		<name>CYCLE_RANGE</name>
		<order>2</order>
		<attributeFromName>
			<groupNumber>1</groupNumber>
			<specification>
				<name>CYCLE_MIN</name>
				<valueType>INTEGER</valueType>
				<calculationClass>fr.cnes.regards.modules.acquisition.plugins.ssalto.calc.SetEnvisatD1BPoeCddisCycleMin</calculationClass>
			</specification>
		</attributeFromName>
        
		<attributeFromName>
			<groupNumber>1</groupNumber>
			<specification>
				<name>CYCLE_MAX</name>
				<valueType>INTEGER</valueType>
				<calculationClass>fr.cnes.regards.modules.acquisition.plugins.ssalto.calc.SetEnvisatD1BPoeCddisCycleMin</calculationClass>
			</specification>
		</attributeFromName>
		
	</compositeAttribute>
	
	
	<fileInfoAttribute>
		<fileInformation>LAST_MODIFICATION_DATE</fileInformation>
		<specification>
			<name>FILE_CREATION_DATE</name>
			<valueType>DATE_TIME</valueType>
			<order>3</order>
		</specification>
	</fileInfoAttribute>
</PLUGIN_CONFIGURATION>
