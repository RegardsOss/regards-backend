<?xml version="1.0" encoding="UTF-8"?>
<PLUGIN_CONFIGURATION xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="schemas_plugins.xsd">

	<fileNamePattern>(H2A_CMR_L2C_BT_V)([0-9]{2})(_([0-9]{8}_[0-9]{6})_([0-9]{8}_[0-9]{6})_([AD]{1}))</fileNamePattern>
<compositeAttribute>
        <name>TIME_PERIOD</name>
        <order>1</order>
        <attributeFromName>
            <groupNumber>4</groupNumber>
            <specification>
                <name>START_DATE</name>
                <valueType>DATE_TIME</valueType>
                <formatRead>yyyyMMdd_HHmmss</formatRead>
            </specification>
        </attributeFromName>
        <attributeFromName>
            <groupNumber>5</groupNumber>
            <specification>
                <name>STOP_DATE</name>
                <valueType>DATE_TIME</valueType>
                <formatRead>yyyyMMdd_HHmmss</formatRead>
            </specification>
        </attributeFromName>
    </compositeAttribute>

   <compositeAttribute>
        <name>CYCLE_RANGE</name>
        <order>3</order>
        <translatedAttributeFromCycleFile>
            <otherAttributeName>START_DATE</otherAttributeName>
            <specification>
                <name>CYCLE_MIN</name>
                <valueType>INTEGER</valueType>
            </specification>
        </translatedAttributeFromCycleFile>
        <translatedAttributeFromCycleFile>
            <otherAttributeName>STOP_DATE</otherAttributeName>
            <specification>
                <name>CYCLE_MAX</name>
                <valueType>INTEGER</valueType>
            </specification>
        </translatedAttributeFromCycleFile>
    </compositeAttribute>

    <translatedAttributeFromName>
        <groupNumber>6</groupNumber>
        <translationFilePath>/RadiometreTranslationFile.properties</translationFilePath>
        <specification>
            <name>PHASE</name>
            <valueType>STRING</valueType>
            <order>4</order>
        </specification>
    </translatedAttributeFromName>

    <attributeFromName>
        <groupNumber>2</groupNumber>
        <specification>
            <name>OBJECT_VERSION</name>
            <valueType>STRING</valueType>
            <order>6</order>
        </specification>
    </attributeFromName>

    <attributeFromName>
        <groupNumber>1</groupNumber>
        <groupNumber>3</groupNumber>
        <specification>
            <name>RADICAL</name>
            <valueType>STRING</valueType>
            <order>7</order>
        </specification>
    </attributeFromName>

    <fileInfoAttribute>
        <fileInformation>LAST_MODIFICATION_DATE</fileInformation>
        <specification>
            <name>FILE_CREATION_DATE</name>
            <valueType>DATE_TIME</valueType>
            <order>8</order>
        </specification>
    </fileInfoAttribute>


</PLUGIN_CONFIGURATION>
