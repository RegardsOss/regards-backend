<?xml version="1.0" encoding="UTF-8"?>
<PLUGIN_CONFIGURATION xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="schemas_plugins.xsd">
	<fileNamePattern>LV(DOR|NAV)1D4([0-9]{2})([0-9]{2})0000</fileNamePattern>	
	<compositeAttribute>
		<name>TIME_PERIOD</name>
		<order>1</order>
		<fileInfoAttribute>
			<fileInformation>LAST_MODIFICATION_DATE</fileInformation>
			<specification>
				<name>START_DATE</name>
				<valueType>DATE_TIME</valueType>
			</specification>
		</fileInfoAttribute>
		<attributeFromOther>
			<otherAttributeName>START_DATE</otherAttributeName>
			<specification>
				<name>STOP_DATE</name>
				<valueType>DATE_TIME</valueType>
			</specification>
		</attributeFromOther>
	</compositeAttribute>
	  <translatedAttributeFromName>
		<groupNumber>1</groupNumber>
		<translationFilePath>/LOGVOL_DORIS_1B_TranslationFile.properties</translationFilePath>
		<specification>
			<name>PRODUCT_OPTION</name>
			<valueType>STRING</valueType>
			<order>2</order>
		</specification>
	</translatedAttributeFromName>
	<attributeFromOther>
		<otherAttributeName>START_DATE</otherAttributeName>
		<specification>
			<name>FILE_CREATION_DATE</name>
			<valueType>DATE_TIME</valueType>
			<order>3</order>
		</specification>
	</attributeFromOther>
	<attributeFromName>
		<groupNumber>2</groupNumber>
		<groupNumber>3</groupNumber>		
		<specification>
			<name>OBJECT_VERSION</name>
			<valueType>STRING</valueType>
			<order>4</order>
			<calculationClass>fr.cnes.regards.modules.acquisition.plugins.ssalto.calc.SetSeparator</calculationClass>
			</specification>
	</attributeFromName>
</PLUGIN_CONFIGURATION>
