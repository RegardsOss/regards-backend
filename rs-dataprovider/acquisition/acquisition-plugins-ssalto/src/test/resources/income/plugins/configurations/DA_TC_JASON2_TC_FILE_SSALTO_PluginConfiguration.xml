<PLUGIN_CONFIGURATION
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="schemas_plugins.xsd">
	<fileNamePattern>
        JA2_TC_(EXCP|ROUTINEJTX|ROUTINEJSEQ)_((DOR|POS)-(1|2))_([a-zA-Z0-9_\-]*)_([0-9]{4}_[0-9]{2}_[0-9]{2}_[0-9]{2}_[0-9]{2}_[0-9]{2})
    </fileNamePattern>
	<compositeAttribute>
		<name>TIME_PERIOD</name>
		<order>1</order>
		<attributeFromName>
			<groupNumber>6</groupNumber>
			<specification>
				<name>START_DATE</name>
				<valueType>DATE_TIME</valueType>
				<formatRead>yyyy_MM_dd_HH_mm_ss</formatRead>
			</specification>
		</attributeFromName>
		<attributeFromName>
			<groupNumber>6</groupNumber>
			<specification>
				<name>STOP_DATE</name>
				<valueType>DATE_TIME</valueType>
				<formatRead>yyyy_MM_dd_HH_mm_ss</formatRead>
			</specification>
		</attributeFromName>
	</compositeAttribute>
	<translatedAttributeFromName>
		<groupNumber>2</groupNumber>
		<translationFilePath>/TC_FILE_SSALTOTranslationFile.properties</translationFilePath>
		<specification>
			<name>PRODUCT_OPTION</name>
			<valueType>STRING</valueType>
			<order>2</order>
		</specification>
	</translatedAttributeFromName>
	<fileInfoAttribute>
		<fileInformation>LAST_MODIFICATION_DATE</fileInformation>
		<specification>
			<name>FILE_CREATION_DATE</name>
			<valueType>DATE_TIME</valueType>
			<order>3</order>
		</specification>
	</fileInfoAttribute>
</PLUGIN_CONFIGURATION>
