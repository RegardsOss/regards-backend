<PLUGIN_CONFIGURATION xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="schemas_plugins.xsd">
  <fileNamePattern>DORDATA_([0-9]{6}).SP2</fileNamePattern>
  <compositeAttribute>
    <name>TIME_PERIOD</name>
    <order>0</order>
    <attributeFromName>
		<groupNumber>1</groupNumber>
			<specification>
				<name>START_DATE</name>
				<valueType>DATE_TIME</valueType>
				<formatRead>yyMMdd</formatRead>
			</specification>
	</attributeFromName>
    <attributeFromName>
		<groupNumber>1</groupNumber>
			<specification>
				<name>STOP_DATE</name>
				<valueType>DATE_TIME</valueType>
				<formatRead>yyMMdd</formatRead>
				<calculationClass>ssalto.domain.plugins.impl.calc.SetStopDateHours</calculationClass>
			</specification>
	</attributeFromName>
  </compositeAttribute>
  <fileInfoAttribute>
			<fileInformation>LAST_MODIFICATION_DATE</fileInformation>
			<specification>
				<name>FILE_CREATION_DATE</name>
				<valueType>DATE_TIME</valueType>
				<order>1</order>
			</specification>
		</fileInfoAttribute>
</PLUGIN_CONFIGURATION>
