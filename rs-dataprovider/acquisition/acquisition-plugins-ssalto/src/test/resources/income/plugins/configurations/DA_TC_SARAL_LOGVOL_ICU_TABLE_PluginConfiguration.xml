<?xml version="1.0" encoding="UTF-8"?>
<PLUGIN_CONFIGURATION xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="schemas_plugins.xsd">

    <fileNamePattern>SRL_TB_[a-zA-Z]{8}_([0-9]{4}_[0-9]{2}_[0-9]{2}_[0-9]{2}_[0-9]{2}_[0-9]{2})</fileNamePattern>

    <compositeAttribute>
        <name>TIME_PERIOD</name>
        <order>1</order>
        <attributeFromName>
            <groupNumber>1</groupNumber>
            <specification>
                <name>START_DATE</name>
                <valueType>DATE_TIME</valueType>
                <formatRead>yyyy_MM_dd_HH_mm_ss</formatRead>
            </specification>
        </attributeFromName>
        <attributeFromName>
            <groupNumber>1</groupNumber>
            <specification>
                <name>STOP_DATE</name>
                <valueType>DATE_TIME</valueType>
                <formatRead>yyyy_MM_dd_HH_mm_ss</formatRead>
            </specification>
        </attributeFromName>
    </compositeAttribute>

    <attributeFromName>
        <groupNumber>1</groupNumber>
        <specification>
            <name>FILE_CREATION_DATE</name>
            <valueType>DATE_TIME</valueType>
            <formatRead>yyyy_MM_dd_HH_mm_ss</formatRead>
        </specification>
    </attributeFromName>

</PLUGIN_CONFIGURATION>
