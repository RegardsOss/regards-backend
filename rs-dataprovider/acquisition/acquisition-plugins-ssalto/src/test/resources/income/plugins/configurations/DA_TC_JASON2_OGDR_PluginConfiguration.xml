<PLUGIN_CONFIGURATION xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="schemas_plugins.xsd">
    <fileNamePattern>(JA2_OP)([NR])(_2P)([a-zA-Z])((S)([0-9]{3})_([0-9]{3})_([0-9]{8}_[0-9]{6})_([0-9]{8}_[0-9]{6})(.nc){0,1})</fileNamePattern>
    <compositeAttribute>
        <name>TIME_PERIOD</name>
        <order>1</order>
        <attributeFromName>
            <groupNumber>9</groupNumber>
            <specification>
                <name>START_DATE</name>
                <valueType>DATE_TIME</valueType>
                <formatRead>yyyyMMdd_HHmmss</formatRead>
            </specification>
        </attributeFromName>
        <attributeFromName>
            <groupNumber>10</groupNumber>
            <specification>
                <name>STOP_DATE</name>
                <valueType>DATE_TIME</valueType>
                <formatRead>yyyyMMdd_HHmmss</formatRead>
            </specification>
        </attributeFromName>
    </compositeAttribute>
    <compositeAttribute>
        <name>CYCLE_RANGE</name>
        <order>2</order>
        <attributeFromName>
            <groupNumber>7</groupNumber>
            <specification>
                <name>CYCLE_MIN</name>
                <valueType>INTEGER</valueType>
            </specification>
        </attributeFromName>
        <attributeFromName>
            <groupNumber>7</groupNumber>
            <specification>
                <name>CYCLE_MAX</name>
                <valueType>INTEGER</valueType>
            </specification>
        </attributeFromName>
    </compositeAttribute>
    <compositeAttribute>
        <name>ORBIT_RANGE</name>
        <order>3</order>
        <attributeFromName>
            <groupNumber>8</groupNumber>
            <specification>
                <name>ORBIT_MIN</name>
                <valueType>INTEGER</valueType>
            </specification>
        </attributeFromName>
        <attributeFromName>
            <groupNumber>8</groupNumber>
            <specification>
                <name>ORBIT_MAX</name>
                <valueType>INTEGER</valueType>
            </specification>
        </attributeFromName>
    </compositeAttribute>
    <translatedAttributeFromName>
        <groupNumber>2</groupNumber>
        <translationFilePath>/OGDRTranslationFile.properties</translationFilePath>
        <specification>
            <name>PRODUCT_OPTION</name>
            <valueType>STRING</valueType>
            <order>4</order>
        </specification>
    </translatedAttributeFromName>
    <attributeFromName>
        <groupNumber>4</groupNumber>
        <specification>
            <name>OBJECT_VERSION</name>
            <valueType>STRING</valueType>
            <order>6</order>
        </specification>
    </attributeFromName>
    <attributeFromName>
        <groupNumber>1</groupNumber>
        <groupNumber>2</groupNumber>
        <groupNumber>3</groupNumber>
        <groupNumber>5</groupNumber>
        <specification>
            <name>RADICAL</name>
            <valueType>STRING</valueType>
            <order>7</order>
        </specification>
    </attributeFromName>
    <compositeAttribute>
        <name>GEO_COORDINATES</name>
        <order>8</order>
        <variableValueAttributeFromCdfFile>
            <variableName>lat</variableName>
            <calculus>min</calculus>
            <specification>
                <name>LATITUDE_MIN</name>
                <valueType>REAL</valueType>
                <calculationClass>fr.cnes.regards.modules.acquisition.plugins.ssalto.calc.FormatLatitude</calculationClass>
            </specification>
        </variableValueAttributeFromCdfFile>
        <variableValueAttributeFromCdfFile>
            <variableName>lon</variableName>
            <calculus>first</calculus>
            <specification>
                <name>LONGITUDE_MIN</name>
                <valueType>REAL</valueType>
                <calculationClass>fr.cnes.regards.modules.acquisition.plugins.ssalto.calc.GreenwichCenterLongitudeCalculation</calculationClass>
            </specification>
        </variableValueAttributeFromCdfFile>
        <variableValueAttributeFromCdfFile>
            <variableName>lat</variableName>
            <calculus>max</calculus>
            <specification>
                <name>LATITUDE_MAX</name>
                <valueType>REAL</valueType>
                <calculationClass>fr.cnes.regards.modules.acquisition.plugins.ssalto.calc.FormatLatitude</calculationClass>
            </specification>
        </variableValueAttributeFromCdfFile>
        <variableValueAttributeFromCdfFile>
            <variableName>lon</variableName>
            <calculus>last</calculus>
            <specification>
                <name>LONGITUDE_MAX</name>
                <valueType>REAL</valueType>
                <calculationClass>fr.cnes.regards.modules.acquisition.plugins.ssalto.calc.GreenwichCenterLongitudeCalculation</calculationClass>
            </specification>
        </variableValueAttributeFromCdfFile>
    </compositeAttribute>
    <!--
    <enumeratedAttributeFromCdfFile>
        <attributeName>long_name</attributeName>
        <except>time</except>
        <except>lon</except>
        <except>lat</except>
        <specification>
            <name>PARAMETER</name>
            <valueType>STRING</valueType>
            <order>9</order>
        </specification>
    </enumeratedAttributeFromCdfFile>
   -->
   
    <globalAttributeFromCdfFile>
        <attributeName>history</attributeName>
        <specification>
            <name>FILE_CREATION_DATE</name>
            <valueType>DATE_TIME</valueType>
            <order>10</order>
            <formatRead>yyyy-MM-dd HH:mm:ss</formatRead>
        </specification>
    </globalAttributeFromCdfFile>
    <attributeFromOther>
        <otherAttributeName>ORBIT_MIN</otherAttributeName>
        <specification>
            <name>PHASE</name>
            <valueType>STRING</valueType>
            <order>11</order>
            <calculationClass>fr.cnes.regards.modules.acquisition.plugins.ssalto.calc.IsPairCalculation</calculationClass>
        </specification>
    </attributeFromOther>
</PLUGIN_CONFIGURATION>
