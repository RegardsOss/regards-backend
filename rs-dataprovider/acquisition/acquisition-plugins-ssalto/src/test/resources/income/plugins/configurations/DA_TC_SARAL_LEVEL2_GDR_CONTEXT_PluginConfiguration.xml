<PLUGIN_CONFIGURATION xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="schemas_plugins.xsd">
        <fileNamePattern>SRL_LEVEL2_GDR_CONTEXT_([0-9]{3})_([0-9]{8}_[0-9]{6})_([0-9]{8}_[0-9]{6})</fileNamePattern>
    <compositeAttribute>
        <name>TIME_PERIOD</name>
        <order>1</order>
        <attributeFromName>
            <groupNumber>2</groupNumber>
            <specification>
                <name>START_DATE</name>
                <valueType>DATE_TIME</valueType>
                <formatRead>yyyyMMdd_HHmmss</formatRead>
            </specification>
        </attributeFromName>
        <attributeFromName>
            <groupNumber>3</groupNumber>
            <specification>
                <name>STOP_DATE</name>
                <valueType>DATE_TIME</valueType>
                <formatRead>yyyyMMdd_HHmmss</formatRead>
            </specification>
        </attributeFromName>
    </compositeAttribute>

   <compositeAttribute>
        <name>CYCLE_RANGE</name>
        <order>2</order>
        <attributeFromName>
            <groupNumber>1</groupNumber>
            <specification>
                <name>CYCLE_MIN</name>
                <valueType>INTEGER</valueType>
            </specification>
        </attributeFromName>
        <attributeFromName>
            <groupNumber>1</groupNumber>
            <specification>
                <name>CYCLE_MAX</name>
                <valueType>INTEGER</valueType>
            </specification>
        </attributeFromName>
    </compositeAttribute>

    <fixedValueAttribute>
        <fixedValue>SRL_LEVEL2_GDR_CONTEXT</fixedValue>
        <specification>
            <name>RADICAL</name>
            <valueType>STRING</valueType>
            <order>3</order>
        </specification>
    </fixedValueAttribute>

    <fileInfoAttribute>
        <fileInformation>LAST_MODIFICATION_DATE</fileInformation>
        <specification>
            <name>FILE_CREATION_DATE</name>
            <valueType>DATE_TIME</valueType>
            <order>4</order>
        </specification>
    </fileInfoAttribute>


</PLUGIN_CONFIGURATION>
