<PLUGIN_CONFIGURATION xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="schemas_plugins.xsd">
    <fileNamePattern>(JA2_HKTMR_P_)([0-9]*)_([0-9]{8}_[0-9]{6})(_([0-9]{8}_[0-9]{6})_([0-9]{8}_[0-9]{6}))</fileNamePattern>
    <compositeAttribute>
        <name>TIME_PERIOD</name>
        <order>1</order>
        <attributeFromName>
            <groupNumber>5</groupNumber>
            <specification>
                <name>START_DATE</name>
                <valueType>DATE_TIME</valueType>
                <formatRead>yyyyMMdd_HHmmss</formatRead>
            </specification>
        </attributeFromName>
        <attributeFromName>
            <groupNumber>6</groupNumber>
            <specification>
                <name>STOP_DATE</name>
                <valueType>DATE_TIME</valueType>
                <formatRead>yyyyMMdd_HHmmss</formatRead>
            </specification>
        </attributeFromName>
    </compositeAttribute>
    <compositeAttribute>
        <name>CYCLE_RANGE</name>
        <order>5</order>
        <translatedAttributeFromCycleFile>
            <otherAttributeName>START_DATE</otherAttributeName>
            <specification>
                <name>CYCLE_MIN</name>
                <valueType>INTEGER</valueType>
            </specification>
        </translatedAttributeFromCycleFile>
        <translatedAttributeFromCycleFile>
            <otherAttributeName>STOP_DATE</otherAttributeName>
            <specification>
                <name>CYCLE_MAX</name>
                <valueType>INTEGER</valueType>
            </specification>
        </translatedAttributeFromCycleFile>
    </compositeAttribute>
    <translatedAttributeFromName>
        <groupNumber>2</groupNumber>
        <translationFilePath>/HKTM_J2_TranslationFile.properties</translationFilePath>
        <specification>
            <name>PRODUCT_OPTION</name>
            <valueType>STRING</valueType>
            <order>4</order>
            <defaultValue>Other</defaultValue>
        </specification>
    </translatedAttributeFromName>

    <attributeFromName>
        <groupNumber>3</groupNumber>
        <specification>
            <name>FILE_CREATION_DATE</name>
            <valueType>DATE_TIME</valueType>
            <order>2</order>
            <formatRead>yyyyMMdd_HHmmss</formatRead>
        </specification>
    </attributeFromName>
</PLUGIN_CONFIGURATION>
