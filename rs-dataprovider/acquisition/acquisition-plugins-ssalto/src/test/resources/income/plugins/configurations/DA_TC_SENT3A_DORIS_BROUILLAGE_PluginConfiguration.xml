<PLUGIN_CONFIGURATION xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="schemas_plugins.xsd">
    <fileNamePattern>(S3A_OPER_DO_0_(1690|1706)__([0-9]{8})T([0-9]{6})_([0-9]{8})T([0-9]{6})_([0-9]{4}))</fileNamePattern>
    <compositeAttribute>
        <name>TIME_PERIOD</name>
        <order>1</order>
        <attributeFromName>
            <groupNumber>3</groupNumber>
            <groupNumber>4</groupNumber>
            <specification>
                <name>START_DATE</name>
                <valueType>DATE_TIME</valueType>
                <formatRead>yyyyMMddHHmmss</formatRead>
            </specification>
        </attributeFromName>
        <attributeFromName>
            <groupNumber>5</groupNumber>
            <groupNumber>6</groupNumber>
            <specification>
                <name>STOP_DATE</name>
                <valueType>DATE_TIME</valueType>
                <formatRead>yyyyMMddHHmmss</formatRead>
            </specification>
        </attributeFromName>
 </compositeAttribute>
 <compositeAttribute>
        <name>CYCLE_RANGE</name>
        <order>2</order>
        <translatedAttributeFromCycleFile>
            <otherAttributeName>START_DATE</otherAttributeName>
            <specification>
                <name>CYCLE_MIN</name>
                <valueType>INTEGER</valueType>
            </specification>
        </translatedAttributeFromCycleFile>
        <translatedAttributeFromCycleFile>
            <otherAttributeName>STOP_DATE</otherAttributeName>
            <specification>
                <name>CYCLE_MAX</name>
                <valueType>INTEGER</valueType>
            </specification>
        </translatedAttributeFromCycleFile>
 </compositeAttribute>
 <attributeFromName>
        <groupNumber>1</groupNumber>
        <specification>
            <name>RADICAL</name>
            <valueType>STRING</valueType>
            <order>3</order>
        </specification>
 </attributeFromName>
 <fileInfoAttribute>
     <fileInformation>LAST_MODIFICATION_DATE</fileInformation>
     <specification>
     	<name>FILE_CREATION_DATE</name>
     	<valueType>DATE_TIME</valueType>
     	<order>4</order>
     </specification>	
 </fileInfoAttribute>
 <attributeFromName>
        <groupNumber>7</groupNumber>
        <specification>
            <name>OBJECT_VERSION</name>
            <valueType>STRING</valueType>
     	    <order>5</order>
        </specification>
 </attributeFromName>
</PLUGIN_CONFIGURATION>
