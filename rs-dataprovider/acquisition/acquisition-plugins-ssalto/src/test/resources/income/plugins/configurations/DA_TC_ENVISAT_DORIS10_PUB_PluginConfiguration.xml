<PLUGIN_CONFIGURATION xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="schemas_plugins.xsd">
	<fileNamePattern>DOR_MEP_1PaD.*</fileNamePattern>
	<compositeAttribute>
		<name>TIME_PERIOD</name>
		<order>1</order>
		<attributeFromAsciiFile>
			<linePattern>.*SENSING_START="([0-9]{2}-.*-[0-9]{4} [0-9]{2}:[0-9]{2}:[0-9]{2})\.[0-9]{6}.*</linePattern>
			<lineNumber>0</lineNumber>
			<groupNumber>1</groupNumber>
			<specification>
				<name>START_DATE</name>
				<valueType>DATE_TIME</valueType>
				<formatRead>dd-MMM-yyyy HH:mm:ss</formatRead>
			</specification>
		</attributeFromAsciiFile>
		<attributeFromAsciiFile>
			<linePattern>.*SENSING_STOP="([0-9]{2}-.*-[0-9]{4} [0-9]{2}:[0-9]{2}:[0-9]{2})\.[0-9]{6}.*</linePattern>
			<lineNumber>0</lineNumber>
			<groupNumber>1</groupNumber>
			<specification>
				<name>STOP_DATE</name>
				<valueType>DATE_TIME</valueType>
				<formatRead>dd-MMM-yyyy HH:mm:ss</formatRead>
			</specification>
		</attributeFromAsciiFile>
	</compositeAttribute>
	<compositeAttribute>
		<name>CYCLE_RANGE</name>
		<order>2</order>
		<translatedAttributeFromCycleFile>
			<otherAttributeName>START_DATE</otherAttributeName>
			<specification>
				<name>CYCLE_MIN</name>
				<valueType>INTEGER</valueType>
			</specification>
		</translatedAttributeFromCycleFile>
		<translatedAttributeFromCycleFile>
			<otherAttributeName>STOP_DATE</otherAttributeName>
			<specification>
				<name>CYCLE_MAX</name>
				<valueType>INTEGER</valueType>
			</specification>
		</translatedAttributeFromCycleFile>
	</compositeAttribute>
	<attributeFromAsciiFile>
		<linePattern>.*PROC_TIME="([0-9]{2}-.*-[0-9]{4} [0-9]{2}:[0-9]{2}:[0-9]{2})\.[0-9]{6}.*</linePattern>
		<lineNumber>0</lineNumber>
		<groupNumber>1</groupNumber>
		<specification>
			<name>FILE_CREATION_DATE</name>
			<valueType>DATE_TIME</valueType>
			<formatRead>dd-MMM-yyyy HH:mm:ss</formatRead>
		</specification>
	</attributeFromAsciiFile>
</PLUGIN_CONFIGURATION>
