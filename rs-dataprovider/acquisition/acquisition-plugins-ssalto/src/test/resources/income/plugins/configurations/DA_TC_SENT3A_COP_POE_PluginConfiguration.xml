<PLUGIN_CONFIGURATION xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="schemas_plugins.xsd">
    <fileNamePattern>(S3A_VCP_AXVCNE)(_){0,1}([0-9]{8}_[0-9]{6})((_){0,1}([0-9]{8}_[0-9]{6})(_){0,1}([0-9]{8}_[0-9]{6}))</fileNamePattern>  
    <compositeAttribute>
        <name>TIME_PERIOD</name>
        <order>3</order>
        <attributeFromName>
            <groupNumber>6</groupNumber>
            <specification>
                <name>START_DATE</name>
                <valueType>DATE_TIME</valueType>
                <formatRead>yyyyMMdd_HHmmss</formatRead>
            </specification>
        </attributeFromName>
        <attributeFromName>
            <groupNumber>8</groupNumber>
            <specification>
                <name>STOP_DATE</name>
                <valueType>DATE_TIME</valueType>
                <formatRead>yyyyMMdd_HHmmss</formatRead>
            </specification>
        </attributeFromName>
    </compositeAttribute>
    <compositeAttribute>
        <name>CYCLE_RANGE</name>
        <order>6</order>
        <translatedAttributeFromCycleFile>
            <otherAttributeName>START_DATE</otherAttributeName>
            <specification>
                <name>CYCLE_MIN</name>
                <valueType>INTEGER</valueType>
            </specification>
        </translatedAttributeFromCycleFile>
        <translatedAttributeFromCycleFile>
            <otherAttributeName>STOP_DATE</otherAttributeName>
            <specification>
                <name>CYCLE_MAX</name>
                <valueType>INTEGER</valueType>
            </specification>
        </translatedAttributeFromCycleFile>
    </compositeAttribute>
    <attributeFromName>
        <groupNumber>1</groupNumber>
        <groupNumber>4</groupNumber>
        <specification>
            <name>RADICAL</name>
            <valueType>STRING</valueType>
            <order>4</order>
        </specification>
    </attributeFromName>
    <attributeFromName>
        <groupNumber>3</groupNumber>
        <specification>
            <name>FILE_CREATION_DATE</name>
            <valueType>DATE_TIME</valueType>
            <order>5</order>
            <formatRead>yyyyMMdd_HHmmss</formatRead>
        </specification>
    </attributeFromName>
</PLUGIN_CONFIGURATION>
