<PLUGIN_CONFIGURATION xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="schemas_plugins.xsd">
    <fileNamePattern>
        R_TCLOG_JA2_(.*)MC_([0-9]{4}_[0-9]{2}_[0-9]{2}_[0-9]{2}_[0-9]{2}_[0-9]{2}).zip
    </fileNamePattern>
    <compositeAttribute>
        <name>TIME_PERIOD</name>
        <order>1</order>
        <multipleFileFromName>
            <fileInZipNamePattern>
            R_TCLOG_JA2_(.*)_([0-9]{4}_[0-9]{2}_[0-9]{2}_[0-9]{2}_[0-9]{2}_[0-9]{2})_([0-9]{4}_[0-9]{2}_[0-9]{2}_[0-9]{2}_[0-9]{2}_[0-9]{2})
        </fileInZipNamePattern>
            <groupNumber>2</groupNumber>
            <calculus>min</calculus>
            <specification>
                <name>START_DATE</name>
                <valueType>DATE_TIME</valueType>
                <formatRead>yyyy_MM_dd_HH_mm_ss</formatRead>
                <unzipBefore>true</unzipBefore>
                <compressionType>ZIP</compressionType>
            </specification>
        </multipleFileFromName>
        <multipleFileFromName>
            <fileInZipNamePattern>
            R_TCLOG_JA2_(.*)_([0-9]{4}_[0-9]{2}_[0-9]{2}_[0-9]{2}_[0-9]{2}_[0-9]{2})_([0-9]{4}_[0-9]{2}_[0-9]{2}_[0-9]{2}_[0-9]{2}_[0-9]{2})
        </fileInZipNamePattern>
            <groupNumber>3</groupNumber>
            <calculus>max</calculus>
            <specification>
                <name>STOP_DATE</name>
                <valueType>DATE_TIME</valueType>
                <formatRead>yyyy_MM_dd_HH_mm_ss</formatRead>
                <unzipBefore>true</unzipBefore>
                <compressionType>ZIP</compressionType>
            </specification>
        </multipleFileFromName>
    </compositeAttribute>
    <attributeFromName>
        <groupNumber>1</groupNumber>
        <specification>
            <name>PRODUCT_OPTION</name>
            <valueType>STRING</valueType>
            <order>3</order>
        </specification>
    </attributeFromName>

    <attributeFromName>
        <groupNumber>2</groupNumber>
        <specification>
            <name>FILE_CREATION_DATE</name>
            <valueType>DATE_TIME</valueType>
            <order>5</order>
            <formatRead>yyyy_MM_dd_HH_mm_ss</formatRead>
        </specification>
    </attributeFromName>

</PLUGIN_CONFIGURATION>
