<PLUGIN_CONFIGURATION xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="schemas_plugins.xsd">
	<fileNamePattern>ENVISAT_CYCLES</fileNamePattern>
	<compositeAttribute>
		<name>TIME_PERIOD</name>
		<order>1</order>
		<fixedValueAttribute>
			<fixedValue>2002-04-10T14:38:56</fixedValue>
			<specification>
				<name>START_DATE</name>
				<valueType>DATE_TIME</valueType>
				<formatRead>yyyy-MM-dd'T'HH:mm:ss</formatRead>
			</specification>
		</fixedValueAttribute>
		<attributeFromAsciiFile>
			<linePattern>\-\s*[0-9]{3}\s*([0-9]{1}.*[0-9]{1})\s*-\s*</linePattern>
			<lineNumber>0</lineNumber>
			<groupNumber>1</groupNumber>
			<specification>
				<name>STOP_DATE</name>
				<valueType>DATE_TIME</valueType>
				<formatRead>dd-MM-yyyy</formatRead>
			</specification>
		</attributeFromAsciiFile>
	</compositeAttribute>
	<fileInfoAttribute>
		<fileInformation>LAST_MODIFICATION_DATE</fileInformation>
		<specification>
			<name>FILE_CREATION_DATE</name>
			<valueType>DATE_TIME</valueType>
			<order>3</order>
		</specification>
	</fileInfoAttribute>
</PLUGIN_CONFIGURATION>
