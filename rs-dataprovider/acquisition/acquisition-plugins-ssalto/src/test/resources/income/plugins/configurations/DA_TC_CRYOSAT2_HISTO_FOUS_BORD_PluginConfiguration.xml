<PLUGIN_CONFIGURATION xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="schemas_plugins.xsd">
	<fileNamePattern>(CS__OS)(1|2)_AXXCNE(_){0,1}([0-9]{8}_[0-9]{6})(_){0,1}([0-9]{8}_[0-9]{6})(_){0,1}([0-9]{8}_[0-9]{6})</fileNamePattern>	
	<compositeAttribute>
		<name>TIME_PERIOD</name>
		<order>3</order>
		<attributeFromName>
			<groupNumber>6</groupNumber>
			<specification>
				<name>START_DATE</name>
				<valueType>DATE_TIME</valueType>
				<formatRead>yyyyMMdd_HHmmss</formatRead>
			</specification>
		</attributeFromName>
		<attributeFromName>
			<groupNumber>8</groupNumber>
			<specification>
				<name>STOP_DATE</name>
				<valueType>DATE_TIME</valueType>
				<formatRead>yyyyMMdd_HHmmss</formatRead>
			</specification>
		</attributeFromName>
	</compositeAttribute>
	<translatedAttributeFromName>
		<groupNumber>2</groupNumber>
		<translationFilePath>/CRYOSAT2_HFB_TranslationFile.properties</translationFilePath>
		<specification>
			<name>PRODUCT_OPTION</name>
			<valueType>STRING</valueType>
			<order>4</order>
		</specification>
	</translatedAttributeFromName>
	<attributeFromName>
		<groupNumber>1</groupNumber>
		<groupNumber>2</groupNumber>
		<specification>
			<name>RADICAL</name>
			<valueType>STRING</valueType>
			<order>5</order>
		</specification>
	</attributeFromName>
	<attributeFromName>
			<groupNumber>4</groupNumber>
			<specification>
				<name>FILE_CREATION_DATE</name>
				<valueType>DATE_TIME</valueType>
				<order>6</order>
				<formatRead>yyyyMMdd_HHmmss</formatRead>
			</specification>
	</attributeFromName>
</PLUGIN_CONFIGURATION>
