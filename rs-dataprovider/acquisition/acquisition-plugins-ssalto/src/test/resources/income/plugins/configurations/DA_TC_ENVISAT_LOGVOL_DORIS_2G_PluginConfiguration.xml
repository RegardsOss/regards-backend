<PLUGIN_CONFIGURATION
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="schemas_plugins.xsd">
	<fileNamePattern>GESTION_DORIS_ENVISAT1([0-9]{5})</fileNamePattern>
	<compositeAttribute>
		<name>TIME_PERIOD</name>
		<order>1</order>
		<attributeFromAsciiFile>
			<linePattern>.*Date de creation : ([0-9]{2}/[0-9]{2}/[0-9]{4} \([0-9]{2}h[0-9]{2}\)).*</linePattern>
			<lineNumber>0</lineNumber>
			<groupNumber>1</groupNumber>
			<specification>
				<name>START_DATE</name>
				<valueType>DATE_TIME</valueType>
				<formatRead>dd/MM/yyyy' ('HH'h'mm')'</formatRead>
			</specification>
		</attributeFromAsciiFile>
		<attributeFromAsciiFile>
            <linePattern>.*Date de creation : ([0-9]{2}/[0-9]{2}/[0-9]{4} \([0-9]{2}h[0-9]{2}\)).*</linePattern>
            <lineNumber>0</lineNumber>
            <groupNumber>1</groupNumber>
            <specification>
                <name>STOP_DATE</name>
                <valueType>DATE_TIME</valueType>
                <formatRead>dd/MM/yyyy' ('HH'h'mm')'</formatRead>
            </specification>
        </attributeFromAsciiFile>
	</compositeAttribute>
        <attributeFromAsciiFile>
            <linePattern>.*Date de creation : ([0-9]{2}/[0-9]{2}/[0-9]{4} \([0-9]{2}h[0-9]{2}\)).*</linePattern>
            <lineNumber>0</lineNumber>
            <groupNumber>1</groupNumber>
            <specification>
                <name>FILE_CREATION_DATE</name>
                <valueType>DATE_TIME</valueType>
                <formatRead>dd/MM/yyyy' ('HH'h'mm')'</formatRead>
            </specification>
        </attributeFromAsciiFile>
	<attributeFromName>
		<groupNumber>1</groupNumber>
		<specification>
			<name>OBJECT_VERSION</name>
			<valueType>STRING</valueType>
			<order>4</order>
			<calculationClass>fr.cnes.regards.modules.acquisition.plugins.ssalto.calc.SetEnvisatLogvolDoris2GSeparator</calculationClass>
			</specification>
	</attributeFromName>        
</PLUGIN_CONFIGURATION>
