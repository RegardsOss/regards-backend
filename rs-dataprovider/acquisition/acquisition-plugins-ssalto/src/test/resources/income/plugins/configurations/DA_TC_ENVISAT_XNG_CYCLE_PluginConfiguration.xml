<PLUGIN_CONFIGURATION xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="schemas_plugins.xsd">
	<fileNamePattern>(EN1_XNG_[OAG]P)([a-zA-Z]{1})(C([0-9]{3}))(.CNES{0,1})</fileNamePattern>
	<compositeAttribute>
		<name>TIME_PERIOD</name>
		<order>1</order>
		<attributeFromAsciiFile>
			<linePattern>First_Measurement_Time_Of_Mission_1\s*=\s*([0-9T:\-]*)\..*</linePattern>
			<lineNumber>0</lineNumber>
			<groupNumber>1</groupNumber>
			<specification>
				<name>START_DATE</name>
				<valueType>DATE_TIME</valueType>
				<formatRead>yyyy-MM-dd'T'HH:mm:ss</formatRead>
			</specification>
		</attributeFromAsciiFile>
		<attributeFromAsciiFile>
			<linePattern>Last_Measurement_Time_Of_Mission_1\s*=\s*([0-9T:\-]*)\..*</linePattern>
			<lineNumber>0</lineNumber>
			<groupNumber>1</groupNumber>
			<specification>
				<name>STOP_DATE</name>
				<valueType>DATE_TIME</valueType>
				<formatRead>yyyy-MM-dd'T'HH:mm:ss</formatRead>
			</specification>
		</attributeFromAsciiFile>
	</compositeAttribute>
	<compositeAttribute>
		<name>CYCLE_RANGE</name>
		<order>2</order>
		<attributeFromName>
			<groupNumber>4</groupNumber>
			<specification>
				<name>CYCLE_MIN</name>
				<valueType>INTEGER</valueType>
			</specification>
		</attributeFromName>
		<attributeFromOther>
			<otherAttributeName>CYCLE_MIN</otherAttributeName>
			<specification>
				<name>CYCLE_MAX</name>
				<valueType>INTEGER</valueType>
			</specification>
		</attributeFromOther>
	</compositeAttribute>
	
	<attributeFromAsciiFile>
			<linePattern>XNG_Surface_Type\s*=\s*([a-zA-Z\-]*);</linePattern>
			<lineNumber>0</lineNumber>
			<groupNumber>1</groupNumber>
			<specification>
				<name>PRODUCT_OPTION</name>
				<valueType>STRING</valueType>
			</specification>
	</attributeFromAsciiFile>
		
	<attributeFromAsciiFile>
		<linePattern>Product_Creation_Time\s*=\s*([0-9T:\-]*)\..*</linePattern>
		<lineNumber>0</lineNumber>
		<groupNumber>1</groupNumber>
		<specification>
			<name>FILE_CREATION_DATE</name>
			<valueType>DATE_TIME</valueType>
			<order>5</order>
			<formatRead>yyyy-MM-dd'T'HH:mm:ss</formatRead>
		</specification>
	</attributeFromAsciiFile>
</PLUGIN_CONFIGURATION>

	
	
