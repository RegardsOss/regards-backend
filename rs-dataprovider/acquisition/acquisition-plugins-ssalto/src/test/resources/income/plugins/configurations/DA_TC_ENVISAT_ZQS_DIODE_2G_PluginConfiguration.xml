<PLUGIN_CONFIGURATION
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="schemas_plugins.xsd">
	<fileNamePattern>z_quasi_stat_DIODE_ENVISAT1_([0-9]{2})</fileNamePattern>
	<compositeAttribute>
		<name>TIME_PERIOD</name>
		<order>3</order>
		<fileInfoAttribute>
		<fileInformation>LAST_MODIFICATION_DATE</fileInformation>
		<specification>
			<name>START_DATE</name>
			<valueType>DATE_TIME</valueType>
			<order>1</order>
		</specification>
	</fileInfoAttribute>
		<fileInfoAttribute>
		<fileInformation>LAST_MODIFICATION_DATE</fileInformation>
		<specification>
			<name>STOP_DATE</name>
			<valueType>DATE_TIME</valueType>
			<order>2</order>
		</specification>
	</fileInfoAttribute>
	</compositeAttribute>
			<fileInfoAttribute>
		<fileInformation>LAST_MODIFICATION_DATE</fileInformation>
		<specification>
			<name>FILE_CREATION_DATE</name>
			<valueType>DATE_TIME</valueType>
			<order>4</order>
		</specification>
	</fileInfoAttribute>
	<attributeFromName>
		<groupNumber>1</groupNumber>
		<specification>
			<name>OBJECT_VERSION</name>
			<valueType>STRING</valueType>
			<order>5</order>
			<calculationClass>fr.cnes.regards.modules.acquisition.plugins.ssalto.calc.SetEnvisatSeparator</calculationClass>
			</specification>
	</attributeFromName>	
