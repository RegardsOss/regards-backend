<PLUGIN_CONFIGURATION xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="schemas_plugins.xsd">
    <fileNamePattern>(CS_OPER_DOR_DOP_0__([0-9]{8})T([0-9]{6})_([0-9]{8})T([0-9]{6}))_([0-9]{4}).HDR</fileNamePattern>
    <compositeAttribute>
        <name>TIME_PERIOD</name>
        <order>3</order>
        <attributeFromName>
            <groupNumber>2</groupNumber>
            <groupNumber>3</groupNumber>
            <specification>
                <name>START_DATE</name>
                <valueType>DATE_TIME</valueType>
                <formatRead>yyyyMMddHHmmss</formatRead>
            </specification>
        </attributeFromName>
        <attributeFromName>
            <groupNumber>4</groupNumber>
            <groupNumber>5</groupNumber>
            <specification>
                <name>STOP_DATE</name>
                <valueType>DATE_TIME</valueType>
                <formatRead>yyyyMMddHHmmss</formatRead>
            </specification>
        </attributeFromName>
    </compositeAttribute>
    <compositeAttribute>
        <name>CYCLE_RANGE</name>
        <order>6</order>
        <translatedAttributeFromCycleFile>
            <otherAttributeName>START_DATE</otherAttributeName>
            <specification>
                <name>CYCLE_MIN</name>
                <valueType>INTEGER</valueType>
            </specification>
        </translatedAttributeFromCycleFile>
        <translatedAttributeFromCycleFile>
            <otherAttributeName>STOP_DATE</otherAttributeName>
            <specification>
                <name>CYCLE_MAX</name>
                <valueType>INTEGER</valueType>
            </specification>
        </translatedAttributeFromCycleFile>
    </compositeAttribute>
    <compositeAttribute>
        <name>ORBIT_RANGE</name>
        <order>10</order>
        <attributeFromAsciiFile>
            <linePattern>\s*&lt;ABS_Orbit_Start&gt;([0-9]*)&lt;/ABS_Orbit_Start&gt;\s*</linePattern>
            <lineNumber>0</lineNumber>
            <groupNumber>1</groupNumber>
            <filterPattern>CS_OPER_DOR_DOP_0_(_[0-9]{8}T[0-9]{6}){2}_[0-9]{4}.HDR</filterPattern>
            <specification>
                <name>ORBIT_MIN</name>
                <valueType>INTEGER</valueType>
            </specification>
        </attributeFromAsciiFile>
        <attributeFromAsciiFile>
            <linePattern>\s*&lt;ABS_Orbit_Stop&gt;([0-9]*)&lt;/ABS_Orbit_Stop&gt;\s*</linePattern>
            <lineNumber>0</lineNumber>
            <groupNumber>1</groupNumber>
            <filterPattern>CS_OPER_DOR_DOP_0_(_[0-9]{8}T[0-9]{6}){2}_[0-9]{4}.HDR</filterPattern>
            <specification>
                <name>ORBIT_MAX</name>
                <valueType>INTEGER</valueType>
            </specification>
        </attributeFromAsciiFile>
    </compositeAttribute>
    <attributeFromName>
        <groupNumber>1</groupNumber>
        <specification>
            <name>RADICAL</name>
            <valueType>STRING</valueType>
            <order>7</order>
        </specification>
    </attributeFromName>
    <attributeFromAsciiFile>
        <linePattern>\s*&lt;Creation_Date&gt;UTC=([0-9]{4}-[0-9]{2}-[0-9]{2})T([0-9]{2}:[0-9]{2}:[0-9]{2})&lt;/Creation_Date&gt;\s*</linePattern>
        <lineNumber>0</lineNumber>
        <groupNumber>1</groupNumber>
        <groupNumber>2</groupNumber>
        <filterPattern>CS_OPER_DOR_DOP_0_(_[0-9]{8}T[0-9]{6}){2}_[0-9]{4}.HDR</filterPattern>
        <specification>
            <name>FILE_CREATION_DATE</name>
            <valueType>DATE_TIME</valueType>
            <order>8</order>
            <formatRead>yyyy-MM-ddHH:mm:ss</formatRead>
        </specification>
    </attributeFromAsciiFile>
    <attributeFromName>
        <groupNumber>6</groupNumber>
        <specification>
            <name>OBJECT_VERSION</name>
            <valueType>STRING</valueType>
        </specification>
    </attributeFromName>
</PLUGIN_CONFIGURATION>
