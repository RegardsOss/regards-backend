<PLUGIN_CONFIGURATION xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="schemas_plugins.xsd">
  <fileNamePattern>SPOT2_ARCS</fileNamePattern>
  <compositeAttribute>
    <name>TIME_PERIOD</name>
    <order>0</order>
    <fixedValueAttribute>
    	<fixedValue>19910612_000000</fixedValue>
   		<specification>
   			<name>START_DATE</name>
   			<valueType>DATE_TIME</valueType>
   			<formatRead>yyyyMMdd_HHmmss</formatRead>
   		</specification>
    </fixedValueAttribute>
	<attributeFromAsciiFile>
		<linePattern>[0-9A-Z]*\s[0-9]*\s[0-9]*\s([0-9]*\s[0-9]*)\s[0-9]*\s[0-9]*\s[0-9]*</linePattern>
		<lineNumber>-2</lineNumber>
		<groupNumber>1</groupNumber>
		<specification>
			<name>STOP_DATE</name>
			<valueType>STRING</valueType>
			<calculationClass>ssalto.domain.plugins.impl.calc.FormatJulianDate</calculationClass>
		</specification>
	</attributeFromAsciiFile>
  </compositeAttribute>
  <fileInfoAttribute>
			<fileInformation>LAST_MODIFICATION_DATE</fileInformation>
			<specification>
				<name>FILE_CREATION_DATE</name>
				<valueType>DATE_TIME</valueType>
				<order>1</order>
			</specification>
		</fileInfoAttribute>
</PLUGIN_CONFIGURATION>
