<PLUGIN_CONFIGURATION xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="schemas_plugins.xsd">
	<fileNamePattern>EN1_XNG_OP[a-zA-Z]{1}7[0-9]{8}(.CNES){0,1}</fileNamePattern>
	<compositeAttribute>
		<name>TIME_PERIOD</name>
		<order>1</order>
		<attributeFromAsciiFile>
			<linePattern>First_Measurement_Time_Of_Mission_2\s*=\s*([0-9T:\-]*)\..*;</linePattern>
			<lineNumber>0</lineNumber>
			<groupNumber>1</groupNumber>
			<specification>
				<name>START_DATE</name>
				<valueType>DATE_TIME</valueType>
				<formatRead>yyyy-MM-dd'T'HH:mm:ss</formatRead>
			</specification>
		</attributeFromAsciiFile>
				<attributeFromAsciiFile>
			<linePattern>Last_Measurement_Time_Of_Mission_2\s*=\s*([0-9T:\-]*)\..*;</linePattern>
			<lineNumber>0</lineNumber>
			<groupNumber>1</groupNumber>
			<specification>
				<name>STOP_DATE</name>
				<valueType>DATE_TIME</valueType>
				<formatRead>yyyy-MM-dd'T'HH:mm:ss</formatRead>
			</specification>
		</attributeFromAsciiFile>
		
	</compositeAttribute>
		<compositeAttribute>
		<name>CYCLE_RANGE</name>
		<order>2</order>
		<attributeFromAsciiFile>
			<linePattern>First_Cycle_Number_Of_Mission_2\s*=\s*([0-9]*);</linePattern>
			<lineNumber>0</lineNumber>
			<groupNumber>1</groupNumber>
			<specification>
				<name>CYCLE_MIN</name>
				<valueType>INTEGER</valueType>
			</specification>
		</attributeFromAsciiFile>
				<attributeFromAsciiFile>
			<linePattern>Last_Cycle_Number_Of_Mission_2\s*=\s*([0-9]*);</linePattern>
			<lineNumber>0</lineNumber>
			<groupNumber>1</groupNumber>
			<specification>
				<name>CYCLE_MAX</name>
				<valueType>INTEGER</valueType>
			</specification>
		</attributeFromAsciiFile>
	</compositeAttribute>
	
		<compositeAttribute>
		<name>ORBIT_RANGE</name>
		<order>3</order>
		<attributeFromAsciiFile>
			<linePattern>First_Pass_Number_Of_Mission_2\s*=\s*([0-9]*);</linePattern>
			<lineNumber>0</lineNumber>
			<groupNumber>1</groupNumber>
			<specification>
				<name>ORBIT_MIN</name>
				<valueType>INTEGER</valueType>
			</specification>
		</attributeFromAsciiFile>
		<attributeFromAsciiFile>
			<linePattern>Last_Pass_Number_Of_Mission_2\s*=\s*([0-9]*);</linePattern>
			<lineNumber>0</lineNumber>
			<groupNumber>1</groupNumber>
			<specification>
				<name>ORBIT_MAX</name>
				<valueType>INTEGER</valueType>
			</specification>
		</attributeFromAsciiFile>
	</compositeAttribute>

	<attributeFromAsciiFile>
			<linePattern>Product_Creation_Time\s*=\s*([0-9T:\-]*)\..*;</linePattern>
			<lineNumber>0</lineNumber>
			<groupNumber>1</groupNumber>
			<specification>
				<name>FILE_CREATION_DATE</name>
				<valueType>DATE_TIME</valueType>
				<formatRead>yyyy-MM-dd'T'HH:mm:ss</formatRead>
			</specification>
		</attributeFromAsciiFile>
</PLUGIN_CONFIGURATION>
