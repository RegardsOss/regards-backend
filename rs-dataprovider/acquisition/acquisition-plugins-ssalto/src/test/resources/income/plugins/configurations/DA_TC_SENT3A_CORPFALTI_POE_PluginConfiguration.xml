<PLUGIN_CONFIGURATION xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="schemas_plugins.xsd">
    <fileNamePattern>(S3A_VPF_AXVCNE)([0-9]{8})_([0-9]{6})(_([0-9]{8})_([0-9]{6})_([0-9]{8})_([0-9]{6}))</fileNamePattern>
    <compositeAttribute>
        <name>TIME_PERIOD</name>
        <order>1</order>
        <attributeFromName>
            <groupNumber>5</groupNumber>
            <groupNumber>6</groupNumber>
            <specification>
                <name>START_DATE</name>
                <valueType>DATE_TIME</valueType>
                <formatRead>yyyyMMddHHmmss</formatRead>
            </specification>
        </attributeFromName>
        <attributeFromName>
            <groupNumber>7</groupNumber>
            <groupNumber>8</groupNumber>
            <specification>
                <name>STOP_DATE</name>
                <valueType>DATE_TIME</valueType>
                <formatRead>yyyyMMddHHmmss</formatRead>
            </specification>
        </attributeFromName>
    </compositeAttribute>
    <compositeAttribute>
        <name>CYCLE_RANGE</name>
        <order>5</order>
        <translatedAttributeFromCycleFile>
            <otherAttributeName>START_DATE</otherAttributeName>
            <specification>
                <name>CYCLE_MIN</name>
                <valueType>INTEGER</valueType>
            </specification>
        </translatedAttributeFromCycleFile>
        <translatedAttributeFromCycleFile>
            <otherAttributeName>STOP_DATE</otherAttributeName>
            <specification>
                <name>CYCLE_MAX</name>
                <valueType>INTEGER</valueType>
            </specification>
        </translatedAttributeFromCycleFile>
    </compositeAttribute>
    <attributeFromName>
        <groupNumber>1</groupNumber>
        <groupNumber>4</groupNumber>
        <specification>
            <name>RADICAL</name>
            <valueType>STRING</valueType>
            <order>3</order>
        </specification>
    </attributeFromName>
    <attributeFromName>
        <groupNumber>2</groupNumber>
        <groupNumber>3</groupNumber>
        <specification>
            <name>FILE_CREATION_DATE</name>
            <valueType>DATE_TIME</valueType>
            <order>2</order>
            <formatRead>yyyyMMddHHmmss</formatRead>
        </specification>
    </attributeFromName>
</PLUGIN_CONFIGURATION>
