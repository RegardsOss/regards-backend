<PLUGIN_CONFIGURATION xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="schemas_plugins.xsd">
	<fileNamePattern>DORDATA_([0-9]{6}).CS2</fileNamePattern>
	<compositeAttribute>
		<name>TIME_PERIOD</name>
		<order>3</order>
		<attributeFromName>
			<groupNumber>1</groupNumber>
			<specification>
				<name>START_DATE</name>
				<valueType>DATE_TIME</valueType>
				<order>1</order>
				<formatRead>yyMMdd</formatRead>
			</specification>
		</attributeFromName>
		<attributeFromName>
			<groupNumber>1</groupNumber>
			<specification>
				<name>STOP_DATE</name>
				<valueType>DATE_TIME</valueType>
				<order>2</order>
				<formatRead>yyMMdd</formatRead>
				<calculationClass>fr.cnes.regards.modules.acquisition.plugins.ssalto.calc.SetStopDateHours</calculationClass>
			</specification>
		</attributeFromName>
	</compositeAttribute>
	<compositeAttribute>
		<name>CYCLE_RANGE</name>
		<order>6</order>
		<translatedAttributeFromCycleFile>
			<otherAttributeName>START_DATE</otherAttributeName>
			<specification>
				<name>CYCLE_MIN</name>
				<valueType>INTEGER</valueType>
				<order>4</order>
			</specification>
		</translatedAttributeFromCycleFile>
		<translatedAttributeFromCycleFile>
			<otherAttributeName>STOP_DATE</otherAttributeName>
			<specification>
				<name>CYCLE_MAX</name>
				<valueType>INTEGER</valueType>
				<order>5</order>
			</specification>
		</translatedAttributeFromCycleFile>
	</compositeAttribute>
	<fileInfoAttribute>
		<fileInformation>LAST_MODIFICATION_DATE</fileInformation>
		<specification>
			<name>FILE_CREATION_DATE</name>
			<valueType>DATE_TIME</valueType>
		</specification>
	</fileInfoAttribute>
</PLUGIN_CONFIGURATION>
