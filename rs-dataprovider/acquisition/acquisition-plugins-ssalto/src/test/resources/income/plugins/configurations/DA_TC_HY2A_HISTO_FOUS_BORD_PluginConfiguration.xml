<PLUGIN_CONFIGURATION xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="schemas_plugins.xsd">
    <fileNamePattern>(H2A_OS([1-2]{1}))_AXXCNE([0-9]{8}_[0-9]{6})(_([0-9]{8}_[0-9]{6})_([0-9]{8}_[0-9]{6}))</fileNamePattern>
    <compositeAttribute>
        <name>TIME_PERIOD</name>
        <order>1</order>
        <attributeFromName>
            <groupNumber>5</groupNumber>
            <specification>
                <name>START_DATE</name>
                <valueType>DATE_TIME</valueType>
                <formatRead>yyyyMMdd_HHmmss</formatRead>
            </specification>
        </attributeFromName>
        <attributeFromName>
            <groupNumber>6</groupNumber>
            <specification>
                <name>STOP_DATE</name>
                <valueType>DATE_TIME</valueType>
                <formatRead>yyyyMMdd_HHmmss</formatRead>
            </specification>
        </attributeFromName>
    </compositeAttribute>
    <translatedAttributeFromName>
        <groupNumber>2</groupNumber>
        <translationFilePath>/HISTOTranslationFile.properties</translationFilePath>
        <specification>
            <name>PRODUCT_OPTION</name>
            <valueType>STRING</valueType>
            <order>2</order>
        </specification>
    </translatedAttributeFromName>
    <attributeFromName>
        <groupNumber>1</groupNumber>
        <specification>
            <name>RADICAL</name>
            <valueType>STRING</valueType>
        </specification>
    </attributeFromName>     
    <attributeFromName>
        <groupNumber>3</groupNumber>
        <specification>
            <name>FILE_CREATION_DATE</name>
            <valueType>DATE_TIME</valueType>
            <order>4</order>
            <formatRead>yyyyMMdd_HHmmss</formatRead>
        </specification>
    </attributeFromName>
</PLUGIN_CONFIGURATION>
