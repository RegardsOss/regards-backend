<PLUGIN_CONFIGURATION xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="schemas_plugins.xsd">
    <fileNamePattern>(ssasrl)([0-9]{2})(.b([0-9]{5}).e([0-9]{5}).(D|_)_(S|_).sp3.Z)</fileNamePattern>
    <compositeAttribute>
        <name>TIME_PERIOD</name>
        <order>1</order>
        <attributeFromName>
            <groupNumber>4</groupNumber>
            <specification>
                <name>START_DATE</name>
                <valueType>DATE_TIME</valueType>
                <formatRead>yyddd</formatRead>
            </specification>
        </attributeFromName>
        <attributeFromName>
            <groupNumber>5</groupNumber>
            <specification>
                <name>STOP_DATE</name>
                <valueType>DATE_TIME</valueType>
                <formatRead>yyddd</formatRead>
            </specification>
        </attributeFromName>
    </compositeAttribute>

   <compositeAttribute>
        <name>CYCLE_RANGE</name>
        <order>2</order>
        <translatedAttributeFromCycleFile>
            <otherAttributeName>START_DATE</otherAttributeName>
            <specification>
                <name>CYCLE_MIN</name>
                <valueType>INTEGER</valueType>
            </specification>
        </translatedAttributeFromCycleFile>
        <translatedAttributeFromCycleFile>
            <otherAttributeName>STOP_DATE</otherAttributeName>
            <specification>
                <name>CYCLE_MAX</name>
                <valueType>INTEGER</valueType>
            </specification>
        </translatedAttributeFromCycleFile>
    </compositeAttribute>

    <fixedValueAttribute>
        <fixedValue>ssasrl</fixedValue>
        <specification>
            <name>RADICAL</name>
            <valueType>STRING</valueType>
            <order>3</order>
        </specification>
    </fixedValueAttribute>

   <fileInfoAttribute>
        <fileInformation>LAST_MODIFICATION_DATE</fileInformation>
        <specification>
            <name>FILE_CREATION_DATE</name>
            <valueType>DATE_TIME</valueType>
            <order>4</order>
        </specification>
    </fileInfoAttribute>


</PLUGIN_CONFIGURATION>
