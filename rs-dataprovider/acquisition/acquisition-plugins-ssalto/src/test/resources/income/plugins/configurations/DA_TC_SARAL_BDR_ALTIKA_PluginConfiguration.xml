<PLUGIN_CONFIGURATION xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="schemas_plugins.xsd">
<fileNamePattern>ALK_BDR_([0-9]{4})_([0-9]{8})_(RAM_DATA|EEPROM)</fileNamePattern>
    <compositeAttribute>
        <name>TIME_PERIOD</name>
        <order>1</order>
        <attributeFromName>
            <groupNumber>2</groupNumber>
            <specification>
                <name>START_DATE</name>
                <valueType>DATE_TIME</valueType>
                <formatRead>yyyyMMdd</formatRead>
            </specification>
        </attributeFromName>
        <attributeFromOther>
            <otherAttributeName>START_DATE</otherAttributeName>
            <specification>
                <name>STOP_DATE</name>
                <valueType>DATE_TIME</valueType>
            </specification>
        </attributeFromOther>
    </compositeAttribute>

    <attributeFromName>
        <groupNumber>3</groupNumber>
        <specification>
            <name>PRODUCT_OPTION</name>
            <valueType>STRING</valueType>
            <order>3</order>
        </specification>
    </attributeFromName>

    <attributeFromName>
        <groupNumber>1</groupNumber>
        <specification>
            <name>OBJECT_VERSION</name>
            <valueType>STRING</valueType>
            <order>5</order>
   	    <calculationClass>fr.cnes.regards.modules.acquisition.plugins.ssalto.calc.SetSeparator</calculationClass>
        </specification>
    </attributeFromName>

    <attributeFromName>
         <groupNumber>2</groupNumber>
         <specification>
             <name>FILE_CREATION_DATE</name>
             <valueType>DATE_TIME</valueType>
             <order>6</order>
             <formatRead>yyyyMMdd</formatRead>
         </specification>
    </attributeFromName>

</PLUGIN_CONFIGURATION>
