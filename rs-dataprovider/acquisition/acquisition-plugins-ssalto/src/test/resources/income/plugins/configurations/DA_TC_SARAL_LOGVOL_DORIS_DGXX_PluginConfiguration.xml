<PLUGIN_CONFIGURATION xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="schemas_plugins.xsd">
	<fileNamePattern>LOGICIEL_DORIS_ALTIKA_([a-zA-Z0-9]{3}_[a-zA-Z0-9]{3})_([0-9]{8}_[0-9]{6})_([A-Z]+).REF</fileNamePattern>
	<compositeAttribute>
		<name>TIME_PERIOD</name>
		<order>1</order>
		<attributeFromName>
			<groupNumber>2</groupNumber>
			<specification>
				<name>START_DATE</name>
				<valueType>DATE_TIME</valueType>
				<formatRead>yyyyMMdd_HHmmss</formatRead>
			</specification>
		</attributeFromName>
		<attributeFromOther>
			<otherAttributeName>START_DATE</otherAttributeName>
			<specification>
				<name>STOP_DATE</name>
				<valueType>DATE_TIME</valueType>
			</specification>
		</attributeFromOther>
	</compositeAttribute>
<attributeFromName>
	<groupNumber>3</groupNumber>
	<specification>
		<name>PRODUCT_OPTION</name>
		<valueType>STRING</valueType>
		<order>2</order>
	</specification>
</attributeFromName>
	<attributeFromAsciiFile>
		<linePattern>\! Creation date : ([0-9]{2}/[0-9]{2}/[0-9]{4})</linePattern>
		<lineNumber>4</lineNumber>
		<groupNumber>1</groupNumber>
		<specification>
			<name>FILE_CREATION_DATE</name>
			<valueType>DATE_TIME</valueType>
			<order>3</order>
			<formatRead>dd/MM/yyyy </formatRead>
		</specification>
	</attributeFromAsciiFile>
	<attributeFromName>
		<groupNumber>1</groupNumber>
		<specification>
			<name>OBJECT_VERSION</name>
			<valueType>STRING</valueType>
			<order>4</order>
		</specification>
	</attributeFromName>
	<attributeFromName>
		<groupNumber>1</groupNumber>
		<specification>
			<name>OBJECT_VERSION</name>
			<valueType>STRING</valueType>
			<order>4</order>
			<calculationClass>fr.cnes.regards.modules.acquisition.plugins.ssalto.calc.SetLogvolDorisDGXXSeparator</calculationClass>
			</specification>
	</attributeFromName>   	
</PLUGIN_CONFIGURATION>
