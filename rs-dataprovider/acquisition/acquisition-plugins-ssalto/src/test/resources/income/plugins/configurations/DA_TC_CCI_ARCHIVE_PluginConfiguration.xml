<?xml version="1.0" encoding="UTF-8"?>
<PLUGIN_CONFIGURATION xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="schemas_plugins.xsd">
	<fileNamePattern>O([0-9]{9}).zip</fileNamePattern>
	<compositeAttribute>
		<name>TIME_PERIOD</name>
		<order>1</order>
		<attributeFromName>
			<groupNumber>1</groupNumber>
			<specification>
				<name>START_DATE</name>
				<valueType>STRING</valueType>
				<calculationClass>fr.cnes.regards.modules.acquisition.plugins.ssalto.calc.SetDateCciArchive</calculationClass>
			</specification>
		</attributeFromName>
		<attributeFromName>
			<groupNumber>1</groupNumber>
			<specification>
				<name>STOP_DATE</name>
				<valueType>STRING</valueType>
				<calculationClass>fr.cnes.regards.modules.acquisition.plugins.ssalto.calc.SetDateCciArchive</calculationClass>
			</specification>
		</attributeFromName>
	</compositeAttribute>
	<attributeFromName>
		<groupNumber>1</groupNumber>
		<specification>
			<name>FILE_CREATION_DATE</name>
			<valueType>STRING</valueType>
			<calculationClass>fr.cnes.regards.modules.acquisition.plugins.ssalto.calc.SetDateCciArchive</calculationClass>
		</specification>
	</attributeFromName>
</PLUGIN_CONFIGURATION>
