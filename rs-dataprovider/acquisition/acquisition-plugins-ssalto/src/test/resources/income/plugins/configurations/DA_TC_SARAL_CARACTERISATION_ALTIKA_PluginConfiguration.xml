<PLUGIN_CONFIGURATION xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="schemas_plugins.xsd">
    <fileNamePattern>(ALK_CHA_AXVCNE)([0-9]{8}_[0-9]{6})(_([0-9]{8}_[0-9]{6})_([0-9]{8}_[0-9]{6}))</fileNamePattern>
    <compositeAttribute>
        <name>TIME_PERIOD</name>
        <order>1</order>
        <attributeFromName>
            <groupNumber>4</groupNumber>
            <specification>
                <name>START_DATE</name>
                <valueType>DATE_TIME</valueType>
                <formatRead>yyyyMMdd_HHmmss</formatRead>
            </specification>
        </attributeFromName>
        <attributeFromName>
            <groupNumber>5</groupNumber>
            <specification>
                <name>STOP_DATE</name>
                <valueType>DATE_TIME</valueType>
                <formatRead>yyyyMMdd_HHmmss</formatRead>
            </specification>
        </attributeFromName>
    </compositeAttribute>

    <attributeFromName>
        <groupNumber>1</groupNumber>
        <groupNumber>3</groupNumber>
        <specification>
            <name>RADICAL</name>
            <valueType>STRING</valueType>
            <order>3</order>
        </specification>
    </attributeFromName>

    <attributeFromName>
        <groupNumber>2</groupNumber>
        <specification>
            <name>FILE_CREATION_DATE</name>
            <valueType>DATE_TIME</valueType>
            <order>2</order>
            <formatRead>yyyyMMdd_HHmmss</formatRead>
        </specification>
    </attributeFromName>

</PLUGIN_CONFIGURATION>
