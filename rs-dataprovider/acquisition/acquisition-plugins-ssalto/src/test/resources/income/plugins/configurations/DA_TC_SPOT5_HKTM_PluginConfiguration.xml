<PLUGIN_CONFIGURATION xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="schemas_plugins.xsd">
	<fileNamePattern>CMP_S5TMSURV_([0-9]{12})</fileNamePattern>	
	<compositeAttribute>
		<name>TIME_PERIOD</name>
		<order>1</order>
		<attributeFromName>
			<groupNumber>1</groupNumber>
			<specification>
				<name>START_DATE</name>
				<valueType>DATE_TIME</valueType>
				<formatRead>yyMMddHHmmss</formatRead>
			</specification>
		</attributeFromName>
		<attributeFromName>
			<groupNumber>1</groupNumber>
			<specification>
				<name>STOP_DATE</name>
				<valueType>DATE_TIME</valueType>
				<formatRead>yyMMddHHmmss</formatRead>
			</specification>
		</attributeFromName>
	</compositeAttribute>
	<attributeFromName>
		<groupNumber>1</groupNumber>
		<specification>
			<name>FILE_CREATION_DATE</name>
			<valueType>DATE_TIME</valueType>
			<order>2</order>
			<formatRead>yyMMddHHmmss</formatRead>
		</specification>
	</attributeFromName>
</PLUGIN_CONFIGURATION>
