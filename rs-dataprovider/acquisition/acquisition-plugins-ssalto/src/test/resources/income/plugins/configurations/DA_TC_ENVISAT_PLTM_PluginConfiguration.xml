<PLUGIN_CONFIGURATION xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="schemas_plugins.xsd">
	<fileNamePattern>DOR_((DOP|NAV)_0PNPD(K|E))([0-9]{8}_[0-9]{6})_[0-9]{9}([0-9]{3})_([0-9]{5})_[0-9]{5}_[0-9]{4}.N1</fileNamePattern>	
	<compositeAttribute>
		<name>TIME_PERIOD</name>
		<order>1</order>
		<attributeFromName>
			<groupNumber>4</groupNumber>
			<specification>
				<name>START_DATE</name>
				<valueType>DATE_TIME</valueType>
				<formatRead>yyyyMMdd_HHmmss</formatRead>
			</specification>
		</attributeFromName>
		<attributeFromAsciiFile>
			<linePattern>SENSING_STOP="([0-9]{1,2}-[A-Z]{3}-[0-9]{4}\s*[0-9]{2}:[0-9]{2}:[0-9]{2}).[0-9]{6}"</linePattern>
			<lineNumber>0</lineNumber>
			<groupNumber>1</groupNumber>
			<specification>
				<name>STOP_DATE</name>
				<valueType>DATE_TIME</valueType>
				<formatRead>dd-MMM-yyyy HH:mm:ss</formatRead>
			</specification>
		</attributeFromAsciiFile>
	</compositeAttribute>
	<compositeAttribute>
		<name>CYCLE_RANGE</name>
		<order>2</order>
		<attributeFromName>
			<groupNumber>5</groupNumber>
			<specification>
				<name>CYCLE_MIN</name>
				<valueType>INTEGER</valueType>
			</specification>
		</attributeFromName>
		<attributeFromName>
			<groupNumber>5</groupNumber>
			<specification>
				<name>CYCLE_MAX</name>
				<valueType>INTEGER</valueType>
			</specification>
		</attributeFromName>
	</compositeAttribute>
	<compositeAttribute>
		<name>ORBIT_RANGE</name>
		<order>3</order>
		<attributeFromName>
			<groupNumber>6</groupNumber>
			<specification>
				<name>ORBIT_MIN</name>
				<valueType>INTEGER</valueType>
			</specification>
		</attributeFromName>
		<attributeFromName>
			<groupNumber>6</groupNumber>
			<specification>
				<name>ORBIT_MAX</name>
				<valueType>INTEGER</valueType>
			</specification>
		</attributeFromName>
	</compositeAttribute>
	<translatedAttributeFromName>
		<groupNumber>1</groupNumber>
		<translationFilePath>/ENVISAT_PLTMTranslationFile.properties</translationFilePath>
		<specification>
			<name>PRODUCT_OPTION</name>
			<valueType>STRING</valueType>
			<order>4</order>
			<defaultValue>Other</defaultValue>
		</specification>
	</translatedAttributeFromName>
	<attributeFromAsciiFile>
		<linePattern>PROC_TIME="([0-9]{1,2}-[A-Z]{3}-[0-9]{4}\s*[0-9]{2}:[0-9]{2}:[0-9]{2}).[0-9]{6}"</linePattern>
		<lineNumber>0</lineNumber>
		<groupNumber>1</groupNumber>
		<specification>
			<name>FILE_CREATION_DATE</name>
			<valueType>DATE_TIME</valueType>
			<order>5</order>
			<formatRead>dd-MMM-yyyy HH:mm:ss</formatRead>
		</specification>
	</attributeFromAsciiFile>
</PLUGIN_CONFIGURATION>
