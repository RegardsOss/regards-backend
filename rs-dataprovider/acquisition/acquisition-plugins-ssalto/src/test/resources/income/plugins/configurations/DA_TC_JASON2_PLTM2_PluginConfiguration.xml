<?xml version="1.0" encoding="UTF-8"?>
<PLUGIN_CONFIGURATION xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="schemas_plugins.xsd">
	<fileNamePattern>
		(JA2_PLTM2_P_)(.*)(MC_)([0-9]{8}_[0-9]{6})(.zip)
	</fileNamePattern>
	<compositeAttribute>
		<name>TIME_PERIOD</name>
		<order>1</order>
		<multipleFileFromName>
			<fileInZipNamePattern>
			(JA2_PLTM2_P_)(.*)_([0-9]{8}_[0-9]{6})_([0-9]{8}_[0-9]{6})_([0-9]{8}_[0-9]{6})
		</fileInZipNamePattern>
			<groupNumber>4</groupNumber>
			<calculus>min</calculus>
			<specification>
				<name>START_DATE</name>
				<valueType>DATE_TIME</valueType>
				<formatRead>yyyyMMdd_HHmmss</formatRead>
				<unzipBefore>true</unzipBefore>
				<compressionType>ZIP</compressionType>
			</specification>
		</multipleFileFromName>
		<multipleFileFromName>
			<fileInZipNamePattern>
			(JA2_PLTM2_P_)(.*)_([0-9]{8}_[0-9]{6})_([0-9]{8}_[0-9]{6})_([0-9]{8}_[0-9]{6})
		</fileInZipNamePattern>
			<groupNumber>5</groupNumber>
			<calculus>max</calculus>
			<specification>
				<name>STOP_DATE</name>
				<valueType>DATE_TIME</valueType>
				<formatRead>yyyyMMdd_HHmmss</formatRead>
				<unzipBefore>true</unzipBefore>
				<compressionType>ZIP</compressionType>
			</specification>
		</multipleFileFromName>
	</compositeAttribute>
	<compositeAttribute>
		<name>CYCLE_RANGE</name>
		<order>3</order>
		<translatedAttributeFromCycleFile>
			<otherAttributeName>START_DATE</otherAttributeName>
			<specification>
				<name>CYCLE_MIN</name>
				<valueType>INTEGER</valueType>
			</specification>
		</translatedAttributeFromCycleFile>
		<translatedAttributeFromCycleFile>
			<otherAttributeName>STOP_DATE</otherAttributeName>
			<specification>
				<name>CYCLE_MAX</name>
				<valueType>INTEGER</valueType>
			</specification>
		</translatedAttributeFromCycleFile>
	</compositeAttribute>
	<attributeFromName>
		<groupNumber>2</groupNumber>
		<specification>
			<name>PRODUCT_OPTION</name>
			<valueType>STRING</valueType>
			<order>4</order>
		</specification>
	</attributeFromName>
	<attributeFromName>
		<groupNumber>4</groupNumber>
		<specification>
			<name>FILE_CREATION_DATE</name>
			<valueType>DATE_TIME</valueType>
			<order>5</order>
			<formatRead>yyyyMMdd_HHmmss</formatRead>
		</specification>
	</attributeFromName>
</PLUGIN_CONFIGURATION>
