<xs:schema xmlns:xs="http://www.w3.org/2001/XMLSchema" elementFormDefault="qualified" attributeFormDefault="unqualified">
	<xs:element name="PLUGIN_CONFIGURATION">
		<xs:annotation>
			<xs:documentation>Root element</xs:documentation>
		</xs:annotation>
		<xs:complexType>
			<xs:sequence>
				<xs:element name="fileNamePattern" type="xs:string">
					<xs:annotation>
						<xs:documentation>file name pattern containing all catch groups to get the required attributes.</xs:documentation>
					</xs:annotation>
				</xs:element>
				<xs:choice maxOccurs="unbounded">
					<xs:element name="compositeAttribute">
						<xs:complexType>
							<xs:sequence>
								<xs:element name="name">
									<xs:simpleType>
										<xs:restriction base="xs:string">
											<xs:enumeration value="TIME_PERIOD"/>
											<xs:enumeration value="ORBIT_RANGE"/>
											<xs:enumeration value="CYCLE_RANGE"/>
											<xs:enumeration value="GEO_COORDINATES"/>
										</xs:restriction>
									</xs:simpleType>
								</xs:element>
								<xs:element name="order" type="xs:integer"/>
								<xs:choice minOccurs="2" maxOccurs="unbounded">
									<xs:element name="attributeFromName" type="attributeFromNameType">
										<xs:annotation>
											<xs:documentation>attribute read from fileName</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="translatedAttributeFromName" type="translatedAttributeFromNameType">
										<xs:annotation>
											<xs:documentation>attribute translated from a value read on the file name</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="translatedAttributeFromCycleFile" type="attributeFromOtherType">
										<xs:annotation>
											<xs:documentation>attribute translated from another attribute value read at groupNumber using the cycle file.</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="multipleFileFromName" type="multipleFileFromNameType">
										<xs:annotation>
											<xs:documentation>attribute read from multiple file names after unzip operation for example</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="attributeFromOther" type="attributeFromOtherType">
										<xs:annotation>
											<xs:documentation>attribute read from another attribute value</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="fileInfoAttribute" type="fileInfoAttributeType">
										<xs:annotation>
											<xs:documentation>attribute read from the File system information ( size ...)</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="globalAttributeFromCdfFile" type="globalAttributeFromCdfFileType">
										<xs:annotation>
											<xs:documentation>global cdf attribute value</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="variableValueAttributeFromCdfFile" type="variableValueAttributeFromCdfFileType">
										<xs:annotation>
											<xs:documentation>value of an attribute of a variable</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="enumeratedAttributeFromCdfFile" type="enumeratedAttributeFromCdfFileType">
										<xs:annotation>
											<xs:documentation>list of all the values of the attribute taken in all variable of the cdf file</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="attributeFromAsciiFile" type="attributeFromAsciiFileType">
										<xs:annotation>
											<xs:documentation>attribute read from an ascii file</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="fixedValueAttribute" type="fixedValueAttributeType">
										<xs:annotation>
											<xs:documentation>attribute with fixed value</xs:documentation>
										</xs:annotation>
									</xs:element>
									<xs:element name="translatedAttributeFromArcFile" type="translatedAttributeFromArcFileType"/>
								</xs:choice>
							</xs:sequence>
						</xs:complexType>
					</xs:element>
					<xs:choice>
						<xs:element name="attributeFromName" type="attributeFromNameType">
							<xs:annotation>
								<xs:documentation>attribute read from fileName</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="translatedAttributeFromName" type="translatedAttributeFromNameType">
							<xs:annotation>
								<xs:documentation>attribute translated from a value read on the file name</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="translatedAttributeFromCycleFile" type="attributeFromOtherType">
							<xs:annotation>
								<xs:documentation>attribute translated from another attribute value read at groupNumber using the cycle file.</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="multipleFileFromName" type="multipleFileFromNameType">
							<xs:annotation>
								<xs:documentation>attribute read from multiple file names after unzip operation for example</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="attributeFromOther" type="attributeFromOtherType">
							<xs:annotation>
								<xs:documentation>attribute read from another attribute value</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="fileInfoAttribute" type="fileInfoAttributeType">
							<xs:annotation>
								<xs:documentation>attribute read from the File system information ( size ...)</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="globalAttributeFromCdfFile" type="globalAttributeFromCdfFileType">
							<xs:annotation>
								<xs:documentation>global cdf attribute value</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="variableValueAttributeFromCdfFile" type="variableValueAttributeFromCdfFileType">
							<xs:annotation>
								<xs:documentation>value of an attribute of a variable</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="enumeratedAttributeFromCdfFile" type="enumeratedAttributeFromCdfFileType">
							<xs:annotation>
								<xs:documentation>list of all the values of the attribute taken in all variable of the cdf file</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="attributeFromAsciiFile" type="attributeFromAsciiFileType">
							<xs:annotation>
								<xs:documentation>attribute read from an ascii file</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="fixedValueAttribute" type="fixedValueAttributeType">
							<xs:annotation>
								<xs:documentation>attribute with fixed value</xs:documentation>
							</xs:annotation>
						</xs:element>
						<xs:element name="translatedAttributeFromArcFile" type="translatedAttributeFromArcFileType"/>
					</xs:choice>
				</xs:choice>
			</xs:sequence>
		</xs:complexType>
	</xs:element>
	<xs:complexType name="SpecificationType">
		<xs:sequence>
			<xs:element name="name">
				<xs:annotation>
					<xs:documentation>attribute name, which will be written into descriptor file</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="FILE_SIZE"/>
						<xs:enumeration value="OBJECT_VERSION"/>
						<xs:enumeration value="LATITUDE_MAX"/>
						<xs:enumeration value="PHASE"/>
						<xs:enumeration value="LATITUDE_MIN"/>
						<xs:enumeration value="LONGITUDE_MAX"/>
						<xs:enumeration value="LONGITUDE_MIN"/>
						<xs:enumeration value="ORBIT_MAX"/>
						<xs:enumeration value="ORBIT_MIN"/>
						<xs:enumeration value="PARAMETER"/>
						<xs:enumeration value="PRODUCT_OPTION"/>
						<xs:enumeration value="FILE_CREATION_DATE"/>
						<xs:enumeration value="CYCLE_MAX"/>
						<xs:enumeration value="CYCLE_MIN"/>
						<xs:enumeration value="STOP_DATE"/>
						<xs:enumeration value="START_DATE"/>
						<xs:enumeration value="RADICAL"/>
						<xs:enumeration value="FULL_RUN_CODE"/>
						<xs:enumeration value="RUN_TYPE"/>
						<xs:enumeration value="FOV"/>
						<xs:enumeration value="ASTBKGROUNDID"/>
						<xs:enumeration value="ASTOFFSETID"/>
						<xs:enumeration value="CCDID"/>
						<xs:enumeration value="COROTID"/>
						<xs:enumeration value="EXOMONOCHROMID"/>
						<xs:enumeration value="HALFCCDID"/>
						<xs:enumeration value="MASKID"/>
						<xs:enumeration value="TPLID"/>
						<xs:enumeration value="WINID"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="valueType">
				<xs:annotation>
					<xs:documentation>type of value ( Date, Integer,String...)</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="STRING"/>
						<xs:enumeration value="DATE"/>
						<xs:enumeration value="INTEGER"/>
						<xs:enumeration value="DATE_TIME"/>
						<xs:enumeration value="REAL"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="order" type="xs:int" minOccurs="0">
				<xs:annotation>
					<xs:documentation>ordering attribute treatment</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="formatRead" minOccurs="0">
				<xs:annotation>
					<xs:documentation>value format read into file or file name</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="calculationClass" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>classe de transformation de la valeur, par exemple transformation des longitude, ou diminution de la precision d'un nombre</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="defaultValue" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>valeur par defaut si le finder n'est pas capable de renvoyer une valeur</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="unzipBefore" type="xs:boolean" minOccurs="0">
				<xs:annotation>
					<xs:documentation>indique si l'on doit dezipper le fichier avant de pouvoir traiter l'attribut</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="compressionType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>indique le type de compression du fichier Z,ZIP,GZIP, TAR</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="ZIP"/>
						<xs:enumeration value="Z"/>
						<xs:enumeration value="GZIP"/>
						<xs:enumeration value="TAR"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="attributeFromNameType">
		<xs:sequence>
			<xs:element name="groupNumber" type="xs:int" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>catch group number</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="unzipBefore" type="xs:boolean" minOccurs="0">
				<xs:annotation>
					<xs:documentation>indique si l'on doit dezipper le fichier avant de pouvoir traiter l'attribut</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="specification" type="SpecificationType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>specification of the attribute</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="translatedAttributeFromNameType">
		<xs:sequence>
			<xs:element name="groupNumber" type="xs:int" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>catch group number</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="translationFilePath" type="xs:string">
				<xs:annotation>
					<xs:documentation>translation file path</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="specification" type="SpecificationType">
				<xs:annotation>
					<xs:documentation>specification of the attribute</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="multipleFileFromNameType">
		<xs:sequence>
			<xs:element name="fileInZipNamePattern" type="xs:string" minOccurs="0">
				<xs:annotation>
					<xs:documentation>pattern representing the file names into the zip archive. </xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="groupNumber" type="xs:int" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>catch group number</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="calculus">
				<xs:annotation>
					<xs:documentation>calculus method to get the value ( max, avg, min...)</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="min"/>
						<xs:enumeration value="max"/>
						<xs:enumeration value="first"/>
						<xs:enumeration value="last"/>
						<xs:enumeration value="average"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="specification" type="SpecificationType">
				<xs:annotation>
					<xs:documentation>specification of the attribute</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="attributeFromOtherType">
		<xs:sequence>
			<xs:element name="otherAttributeName" type="xs:string" minOccurs="0"/>
			<xs:element name="groupNumber" type="xs:int" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>catch group number</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="specification" type="SpecificationType" minOccurs="0">
				<xs:annotation>
					<xs:documentation>specification of the attribute</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="fileInfoAttributeType">
		<xs:sequence>
			<xs:element name="fileInformation">
				<xs:annotation>
					<xs:documentation>the name of the information to read</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="FILE_SIZE"/>
						<xs:enumeration value="LAST_MODIFICATION_DATE"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="specification" type="SpecificationType">
				<xs:annotation>
					<xs:documentation>specification of the attribute</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="globalAttributeFromCdfFileType">
		<xs:sequence>
			<xs:element name="attributeName" type="xs:string">
				<xs:annotation>
					<xs:documentation>name of the global attribute</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="specification" type="SpecificationType">
				<xs:annotation>
					<xs:documentation>specification of the attribute</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="variableValueAttributeFromCdfFileType">
		<xs:sequence>
			<xs:element name="variableName" type="xs:string">
				<xs:annotation>
					<xs:documentation>variable name</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="calculus">
				<xs:annotation>
					<xs:documentation>calculus method to get the value ( max, avg, min...)</xs:documentation>
				</xs:annotation>
				<xs:simpleType>
					<xs:restriction base="xs:string">
						<xs:enumeration value="min"/>
						<xs:enumeration value="max"/>
						<xs:enumeration value="first"/>
						<xs:enumeration value="last"/>
						<xs:enumeration value="average"/>
					</xs:restriction>
				</xs:simpleType>
			</xs:element>
			<xs:element name="specification" type="SpecificationType">
				<xs:annotation>
					<xs:documentation>specification of the attribute</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="enumeratedAttributeFromCdfFileType">
		<xs:sequence>
			<xs:element name="attributeName" type="xs:string">
				<xs:annotation>
					<xs:documentation>attribute name</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="except" type="xs:string" minOccurs="0" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>list of values not to add into the list</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="specification" type="SpecificationType">
				<xs:annotation>
					<xs:documentation>specification of the attribute</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="attributeFromAsciiFileType">
		<xs:sequence>
			<xs:element name="linePattern" type="xs:string">
				<xs:annotation>
					<xs:documentation>pattern representing the line to read and get the value from</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="lineNumber" type="xs:int" minOccurs="0">
				<xs:annotation>
					<xs:documentation>number of the line which contains the value</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="groupNumber" type="xs:string" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>number of the group in the pattern used to catch the value</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="filterPattern" minOccurs="0">
				<xs:annotation>
					<xs:documentation>Filter to select file in multiple file product</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="specification" type="SpecificationType">
				<xs:annotation>
					<xs:documentation>specification of the attribute</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="fixedValueAttributeType">
		<xs:sequence>
			<xs:element name="fixedValue" type="xs:string"/>
			<xs:element name="specification" type="SpecificationType">
				<xs:annotation>
					<xs:documentation>specification of the attribute</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
	<xs:complexType name="translatedAttributeFromArcFileType">
		<xs:sequence>
			<xs:element name="groupNumber" type="xs:int" maxOccurs="unbounded">
				<xs:annotation>
					<xs:documentation>catch group number</xs:documentation>
				</xs:annotation>
			</xs:element>
			<xs:element name="specification" type="SpecificationType">
				<xs:annotation>
					<xs:documentation>specification of the attribute</xs:documentation>
				</xs:annotation>
			</xs:element>
		</xs:sequence>
	</xs:complexType>
</xs:schema>
