<PLUGIN_CONFIGURATION xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="schemas_plugins.xsd">
    <fileNamePattern>S3A_SR_2_PGI_AX_([0-9]{8})T([0-9]{6})_([0-9]{8})T([0-9]{6})_([0-9]{8})T([0-9]{6})___________________CNE_O_NR____.HDR</fileNamePattern>
    <compositeAttribute>
        <name>TIME_PERIOD</name>
        <order>1</order>
        <attributeFromName>
            <groupNumber>1</groupNumber>
            <groupNumber>2</groupNumber>
            <specification>
                <name>START_DATE</name>
                <valueType>DATE_TIME</valueType>
                <formatRead>yyyyMMddHHmmss</formatRead>
            </specification>
        </attributeFromName>
        <attributeFromName>
            <groupNumber>3</groupNumber>
            <groupNumber>4</groupNumber>
            <specification>
                <name>STOP_DATE</name>
                <valueType>DATE_TIME</valueType>
                <formatRead>yyyyMMddHHmmss</formatRead>
            </specification>
        </attributeFromName>
    </compositeAttribute>
    <compositeAttribute>
             <name>CYCLE_RANGE</name>
                     <order>3</order>
                             <translatedAttributeFromCycleFile>
                                         <otherAttributeName>START_DATE</otherAttributeName>
                                         <specification>
                                                    <name>CYCLE_MIN</name>
                                                    <valueType>INTEGER</valueType>
                                         </specification>
                            </translatedAttributeFromCycleFile>
                            <translatedAttributeFromCycleFile>
                                   <otherAttributeName>STOP_DATE</otherAttributeName>
                                   <specification>
                                   	<name>CYCLE_MAX</name>
                                   	<valueType>INTEGER</valueType>
                                   </specification>
                            </translatedAttributeFromCycleFile>
    </compositeAttribute>
    <fixedValueAttribute>
        <fixedValue>S3A_SR_2_PGI_AX</fixedValue>
        <specification>
            <name>RADICAL</name>
            <valueType>STRING</valueType>
             <order>4</order>
        </specification>
    </fixedValueAttribute>
    <attributeFromName>
            <groupNumber>5</groupNumber>
            <groupNumber>6</groupNumber>
            <specification>
            	<name>FILE_CREATION_DATE</name>
                <valueType>DATE_TIME</valueType>
                <formatRead>yyyyMMddHHmmss</formatRead>
            </specification>
    </attributeFromName>
    <attributeFromAsciiFile>
               <linePattern>\s*&lt;File_Version&gt;([0-9]{4})&lt;/File_Version&gt;\s*</linePattern>
         <lineNumber>0</lineNumber>
          <groupNumber>1</groupNumber>
          <filterPattern>S3A_SR_2_PGI_AX_([0-9]{8})T([0-9]{6})_([0-9]{8})T([0-9]{6})_([0-9]{8})T([0-9]{6})___________________CNE_O_NR____.HDR</filterPattern>
        <specification>
            <name>OBJECT_VERSION</name>
            <valueType>STRING</valueType>
             <order>5</order>
         </specification>
    </attributeFromAsciiFile>

</PLUGIN_CONFIGURATION>
