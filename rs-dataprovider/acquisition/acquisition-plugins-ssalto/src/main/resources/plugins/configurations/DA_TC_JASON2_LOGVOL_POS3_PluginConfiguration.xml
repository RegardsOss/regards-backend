<?xml version="1.0" encoding="UTF-8"?>
<PLUGIN_CONFIGURATION xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="schemas_plugins.xsd">
	<fileNamePattern>POS3_([1-2]{1})_LV_([0-9]{4})_([0-9]{8})_[A-Z]+_[A-Z]{3}</fileNamePattern>	
	<compositeAttribute>
		<name>TIME_PERIOD</name>
		<order>1</order>
		<attributeFromName>
			<groupNumber>3</groupNumber>
			<specification>
				<name>START_DATE</name>
				<valueType>DATE_TIME</valueType>
				<formatRead>yyyyMMdd</formatRead>
			</specification>
		</attributeFromName>
		<attributeFromName>
			<groupNumber>3</groupNumber>
			<specification>
				<name>STOP_DATE</name>
				<valueType>DATE_TIME</valueType>
				<formatRead>yyyyMMdd</formatRead>
				<calculationClass>fr.cnes.regards.modules.acquisition.plugins.ssalto.calc.SetStopDateHours</calculationClass>
			</specification>
		</attributeFromName>
	</compositeAttribute>
	<translatedAttributeFromName>
		<groupNumber>1</groupNumber>
		<translationFilePath>/LOGVOLPOS3TranslationFile.properties</translationFilePath>
		<specification>
			<name>PRODUCT_OPTION</name>
			<valueType>STRING</valueType>
			<order>2</order>
		</specification>
	</translatedAttributeFromName>
	<attributeFromName>
		<groupNumber>2</groupNumber>
		<specification>
			<name>OBJECT_VERSION</name>
			<valueType>STRING</valueType>
			<order>3</order>
		</specification>
	</attributeFromName>
	<fileInfoAttribute>
		<fileInformation>LAST_MODIFICATION_DATE</fileInformation>
		<specification>
			<name>FILE_CREATION_DATE</name>
			<valueType>DATE_TIME</valueType>
			<order>4</order>
		</specification>
	</fileInfoAttribute>
</PLUGIN_CONFIGURATION>
