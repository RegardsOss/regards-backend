<PLUGIN_CONFIGURATION xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="schemas_plugins.xsd">
    <fileNamePattern>(S3A_OPER_CRF_SAL[0-9]{3}_([0-9]{7})T([0-9]{6})_([0-9]{7})T([0-9]{6}))_(0001).HDR</fileNamePattern>

   <compositeAttribute>
       <name>TIME_PERIOD</name>
       <order>1</order>
       <attributeFromName>
       	<groupNumber>2</groupNumber>
       	<groupNumber>3</groupNumber>
       	<specification>
              <name>START_DATE</name>
              <valueType>DATE_TIME</valueType>
              <formatRead>yyyydddHHmmss</formatRead>
       	</specification>
       </attributeFromName>
       <attributeFromName>
       	<groupNumber>4</groupNumber>
       	<groupNumber>5</groupNumber>
       	<specification>
              <name>STOP_DATE</name>
              <valueType>DATE_TIME</valueType>
              <formatRead>yyyydddHHmmss</formatRead>
       	</specification>
       </attributeFromName>
   </compositeAttribute>

    <attributeFromName>
        <groupNumber>2</groupNumber>
        <groupNumber>3</groupNumber>
        <specification>
            <name>FILE_CREATION_DATE</name>
            <valueType>DATE_TIME</valueType>
            <formatRead>yyyydddHHmmss</formatRead>
            <order>3</order>
        </specification>
    </attributeFromName>

    <attributeFromName>
        <groupNumber>1</groupNumber>
        <specification>
            <name>RADICAL</name>
            <valueType>STRING</valueType>
            <order>4</order>
        </specification>
    </attributeFromName>

    <attributeFromName>
        <groupNumber>6</groupNumber>
        <specification>
            <name>OBJECT_VERSION</name>
            <valueType>STRING</valueType>
            <order>5</order>
        </specification>
    </attributeFromName>

</PLUGIN_CONFIGURATION>
