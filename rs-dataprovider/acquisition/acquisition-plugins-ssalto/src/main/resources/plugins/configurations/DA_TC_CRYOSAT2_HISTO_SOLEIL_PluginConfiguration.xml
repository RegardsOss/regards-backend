<?xml version="1.0" encoding="UTF-8"?>
<PLUGIN_CONFIGURATION xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="schemas_plugins.xsd">
	<fileNamePattern>CS_OPER_AUX_SUNACT_([0-9]{8})T([0-9]{6})_([0-9]{8})T([0-9]{6})_([0-9]{4}).HDR</fileNamePattern>	
	<compositeAttribute>
		<name>TIME_PERIOD</name>
		<order>3</order>
		<attributeFromName>
			<groupNumber>1</groupNumber>
			<groupNumber>2</groupNumber>
			<specification>
				<name>START_DATE</name>
				<valueType>DATE_TIME</valueType>
				<formatRead>yyyyMMddHHmmss</formatRead>
			</specification>
		</attributeFromName>
		<attributeFromName>
			<groupNumber>3</groupNumber>
			<groupNumber>4</groupNumber>
			<specification>
				<name>STOP_DATE</name>
				<valueType>DATE_TIME</valueType>
				<formatRead>yyyyMMddHHmmss</formatRead>
			</specification>
		</attributeFromName>
	</compositeAttribute>
	<fixedValueAttribute>
		<fixedValue>CS_OPER_AUX_SUNACT</fixedValue>
		<specification>
			<name>RADICAL</name>
			<valueType>STRING</valueType>
			<order>4</order>
		</specification>
	</fixedValueAttribute>
	<attributeFromAsciiFile>
		<linePattern>\s*&lt;Creation_Date&gt;UTC=([0-9]{4}-[0-9]{2}-[0-9]{2})T([0-9]{2}:[0-9]{2}:[0-9]{2})&lt;/Creation_Date&gt;\s*</linePattern>
		<lineNumber>0</lineNumber>
		<groupNumber>1</groupNumber>
		<groupNumber>2</groupNumber>
		<filterPattern>CS_OPER_AUX_SUNACT(_[0-9]{8}T[0-9]{6}){2}_[0-9]{4}.HDR</filterPattern>
		<specification>
			<name>FILE_CREATION_DATE</name>
			<valueType>DATE_TIME</valueType>
			<order>8</order>
			<formatRead>yyyy-MM-ddHH:mm:ss</formatRead>
		</specification>
	</attributeFromAsciiFile>
	<attributeFromName>
		<groupNumber>5</groupNumber>
		<specification>
			<name>OBJECT_VERSION</name>
			<valueType>STRING</valueType>
		</specification>
	</attributeFromName>
</PLUGIN_CONFIGURATION>
