<?xml version="1.0" encoding="UTF-8"?>
<model xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="../../main/resources/model_V1.0.xsd">
	<name>sample</name>
	<description>Sample mission</description>
	<type>COLLECTION</type>
	<attribute alterable="false" optional="false" queryable="false"
		facetable="false">
		<name>att_string</name>
		<type>STRING</type>
		<computationMode>GIVEN</computationMode>
	</attribute>
	<attribute alterable="false" optional="false" queryable="false"
		facetable="false">
		<name>att_boolean</name>
		<type>BOOLEAN</type>
		<computationMode>GIVEN</computationMode>
	</attribute>
	<attribute>
		<name>floatnb</name>
		<description>one floating number</description>
		<type>DOUBLE</type>
		<restriction>
			<double_range>
				<min excluded="false">10</min>
				<max excluded="false">20</max>
			</double_range>
		</restriction>
		<computationMode>GIVEN</computationMode>
	</attribute>
	<attribute>
		<name>integer_range</name>
		<description>an integer range</description>
		<type>DOUBLE</type>
		<restriction>
			<integer_range>
				<min excluded="true">20</min>
				<max>30</max>
			</integer_range>
		</restriction>
		<computationMode>GIVEN</computationMode>
	</attribute>
	<fragment>
		<name>GEO</name>
		<description>Geographic information</description>
		<attribute alterable="false" optional="false" queryable="false"
			facetable="false">
			<name>CRS</name>
			<type>STRING</type>
			<restriction>
				<enumeration>
					<value>Earth</value>
					<value>Mars</value>
					<value>Venus</value>
				</enumeration>
			</restriction>
			<computationMode>GIVEN</computationMode>
		</attribute>
		<attribute alterable="false" optional="false" queryable="false"
			facetable="false">
			<name>GEOMETRY</name>
			<type>GEOMETRY</type>
			<computationMode>GIVEN</computationMode>
		</attribute>
	</fragment>
	<fragment>
		<name>Contact</name>
		<description>Contact information</description>
		<attribute alterable="false" optional="false" queryable="false"
			facetable="false">
			<name>Phone</name>
			<type>STRING</type>
			<restriction>
				<pattern>[0-9 ]{10}</pattern>
			</restriction>
			<computationMode>GIVEN</computationMode>
		</attribute>
	</fragment>
</model>