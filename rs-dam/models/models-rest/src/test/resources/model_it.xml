<?xml version="1.0" encoding="UTF-8"?>
<model xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="../../../../models-domain/src/main/resources/model_V1.0.xsd">
	<name>sample</name>
	<description>Sample mission</description>
	<type>COLLECTION</type>
	<attribute alterable="false" optional="true">
		<label>forTests</label>
		<name>att_string</name>
		<type>STRING</type>
		<computationMode>GIVEN</computationMode>
	</attribute>
	<fragment>
		<name>GEO</name>
		<description>Geographic information</description>
		<attribute alterable="false" optional="false">
			<label>forTests</label>
			<name>CRS</name>
			<type>STRING</type>
			<restriction>
				<enumeration>
					<value>Earth</value>
					<value>Mars</value>
					<value>Venus</value>
				</enumeration>
			</restriction>
			<computationMode>GIVEN</computationMode>
		</attribute>
	</fragment>
	<fragment>
		<name>Contact</name>
		<description>Contact information</description>
		<attribute alterable="true" optional="true">
			<label>forTests</label>
			<name>Phone</name>
			<type>STRING</type>
			<restriction>
				<pattern>[0-9 ]{10}</pattern>
			</restriction>
			<computationMode>COMPUTED</computationMode>
			<configurationLabel>ContactPluginConfTest</configurationLabel>
		</attribute>
	</fragment>
</model>