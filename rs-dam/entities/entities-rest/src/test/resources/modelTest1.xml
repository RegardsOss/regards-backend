<?xml version="1.0" encoding="UTF-8"?>
<model xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:noNamespaceSchemaLocation="model_V1.0.xsd">
	<name>MISSION</name>
	<description>Sample mission</description>
	<type>COLLECTION</type>
	<attribute>
		<name>reference</name>
		<type>STRING</type>
	</attribute>
	<attribute>
		<name>active</name>
		<type>BOOLEAN</type>
	</attribute>
	<fragment>
		<name>geo</name>
		<description>Geographic information</description>
		<attribute>
			<name>crs</name>
			<type>STRING</type>
			<restriction>
				<enumeration>
					<value>Earth</value>
					<value>Mars</value>
					<value>Venus</value>
				</enumeration>
			</restriction>
		</attribute>
	</fragment>
</model>