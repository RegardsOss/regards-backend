<?xml version="1.0" encoding="UTF-8"?>
<model xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
       xsi:noNamespaceSchemaLocation="model_V1.0.xsd">
    <name>DATASET_ENGINE</name>
    <type>DATASET</type>
    <attribute indexed="true">
        <label>Star system name</label>
        <name>starSystem</name>
        <type>STRING</type>
    </attribute>
    <attribute indexed="true">
        <label>Number of planets</label>
        <name>numberOfPlanets</name>
        <type>INTEGER</type>
    </attribute>
    <attribute indexed="true">
        <label>Distance to solar system (Light year)</label>
        <name>distanceToSolarSystem</name>
        <type>DOUBLE</type>
    </attribute>
    <attribute indexed="true">
        <label>Beginning of the study</label>
        <name>studyDate</name>
        <type>DATE_ISO8601</type>
    </attribute>
    <attribute indexed="true">
        <label>Associated laboratory</label>
        <name>researchLab</name>
        <type>URL</type>
    </attribute>
</model>