  <xs:complexType name="EOS_Platform_Type">
    <xs:annotation>
      <xs:documentation>Description: extends MI_Platform by adding EOS_Instrument and additionalAttributes</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="gmi:MI_Platform_Type">
        <xs:sequence>
          <!-- An EOS_Platform must have an EOS_Instrument. This complicates the EOS_Platform because it includes elements 
            named instrument from two namespaces (gmi and eos). The gmi instrument is required, so it must be included
            with a gco:nilReason in instance documents.
          -->
          <!--<xs:element name="instrument" type="eos:EOS_Instrument_PropertyType" minOccurs="0" maxOccurs="unbounded"/>-->
          <xs:element name="otherPropertyType" type="gco:RecordType_PropertyType" minOccurs="0"/>
          <xs:element name="otherProperty" type="gco:Record_PropertyType" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <!-- ......................................................................................... -->
  <xs:element name="EOS_Platform" type="eos:EOS_Platform_Type" substitutionGroup="gmi:MI_Platform"/>
  <!-- ......................................................................................... -->
  <xs:complexType name="EOS_Platform_PropertyType">
    <xs:sequence>
      <xs:element ref="eos:EOS_Platform" minOccurs="0"/>
    </xs:sequence>
    <xs:attributeGroup ref="gco:ObjectReference"/>
    <xs:attribute ref="gco:nilReason"/>
  </xs:complexType>
  <!-- =========================================================================== -->
  <xs:complexType name="EOS_Instrument_Type">
    <xs:annotation>
      <xs:documentation>Description: extends MI_Instrument by adding sensor and additionalAttributes</xs:documentation>
    </xs:annotation>
    <xs:complexContent>
      <xs:extension base="gmi:MI_Instrument_Type">
        <xs:sequence>
          <!-- An EOS_Instrument must be mounted on an EOS_Platform Ted Habermann 2014-01-24
            This complicates the EOS_Instrument because it includes elements 
            named mountedOn from two namespaces (gmi and eos). If the instrument referenced by
            the mountedOn is an EOS_Instrument the role eos:mountedOn must be used
          -->
          <!--<xs:element name="mountedOn" type="eos:EOS_Platform_PropertyType" minOccurs="0" maxOccurs="1"/>-->          
          <xs:element name="otherPropertyType" type="gco:RecordType_PropertyType" minOccurs="0"/>
          <xs:element name="otherProperty" type="gco:Record_PropertyType" minOccurs="0" maxOccurs="unbounded"/>
          <xs:element name="sensor" type="eos:EOS_Sensor_PropertyType" minOccurs="0" maxOccurs="unbounded"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
