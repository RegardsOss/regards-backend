<?xml version="1.0" encoding="UTF-8" ?>
<model xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="model_V1.0.xsd">
    <name>FEATURE01</name>
    <description>Test model</description>
    <version>0.0.1</version>
    <type>DATA</type>
    <attribute alterable="false">
        <label>Data type</label>
        <name>data_type</name>
        <type>STRING</type>
        <restriction>
            <enumeration>
                <value>TYPE01</value>
                <value>TYPE02</value>
                <value>TYPE03</value>
            </enumeration>
        </restriction>
    </attribute>
    <attribute optional="true">
        <label>Label</label>
        <name>label</name>
        <type>STRING</type>
        <restriction>
            <pattern>^.*$</pattern>
        </restriction>
    </attribute>
    <!-- File characterization -->
    <fragment>
        <name>file_characterization</name>
        <attribute>
            <label>Validity flag</label>
            <name>valid</name>
            <type>BOOLEAN</type>
        </attribute>
        <attribute optional="true">
            <label>Invalidation date</label>
            <name>invalidation_date</name>
            <type>DATE_ISO8601</type>
        </attribute>
    </fragment>
</model>