<?xml version="1.0" encoding="UTF-8" ?>
<!-- 
	Last change : 10/03/2020
-->
<model xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="model_V1.0.xsd">
	<name>GEODE001</name>
	<description>Data model for SWOT data lifecycle</description>
	<version>1.0.0</version>
	<type>DATA</type>
	<!-- System infos -->
	<fragment>
		<name>system</name>
		<attribute>
			<label>File size</label>
			<name>filesize</name>
			<type>INTEGER</type>
		</attribute>
		<attribute>
			<label>Ingestion date</label>
			<name>ingestion_date</name>
			<description>Ingestion date in the GEODE repository</description>
			<type>DATE_ISO8601</type>
		</attribute>
		<attribute>
			<label>Change date</label>
			<name>change_date</name>
			<type>DATE_ISO8601</type>
		</attribute>
		<attribute optional="true">
			<label>REGARDS URL</label>
			<name>regards_url</name>
			<type>STRING</type>
			<restriction>
				<pattern>^(https?|ftp|file)://[-a-zA-Z0-9+&#38;@#/%?=~_|!:,.;]*[-a-zA-Z0-9+&#38;@#/%=~_|]</pattern>
			</restriction>
		</attribute>
		<attribute optional="true">
			<label>GPFS URL</label>
			<name>gpfs_url</name>
			<type>STRING</type>
			<restriction>
				<pattern>^(https?|ftp|file)://[-a-zA-Z0-9+&#38;@#/%?=~_|!:,.;]*[-a-zA-Z0-9+&#38;@#/%=~_|]</pattern>
			</restriction>
		</attribute>
		<attribute optional="true">
			<label>LOM URL</label>
			<name>lom_url</name>
			<type>STRING</type>
			<restriction>
				<pattern>^(https?|ftp|file)://[-a-zA-Z0-9+&#38;@#/%?=~_|!:,.;]*[-a-zA-Z0-9+&#38;@#/%=~_|]</pattern>
			</restriction>
		</attribute>
		<attribute>
			<label>Filename</label>
			<name>filename</name>
			<type>STRING</type>
		</attribute>
		<attribute optional="true">
			<label>Checksum</label>
			<name>checksum</name>
			<type>STRING</type>
		</attribute>
		<attribute>
			<label>Extension</label>
			<name>extension</name>
			<type>STRING</type>
			<restriction>
				<enumeration>
					<value>tar</value>
					<value>nc</value>
					<value>shp</value>
					<value>zip</value>
					<value>tgz</value>
					<value>xml</value>
				</enumeration>
			</restriction>
		</attribute>
	</fragment>
	<!-- Data -->
	<fragment>
		<name>data</name>
		<attribute>
			<label>Data type</label>
			<name>type</name>
			<type>STRING</type>
			<restriction>
				<enumeration>
					<value>L0A_LR_Packet</value>
					<value>L2_LR_SSH</value>
					<value>XDF_SURFACE_PRESSURE_ANA</value>
					<value>L2_HR_RIVER_SP</value>
				</enumeration>
			</restriction>
		</attribute>
		<attribute optional="true">
			<label>Production date</label>
			<name>production_date</name>
			<type>DATE_ISO8601</type>
		</attribute>
		<attribute optional="true">
			<label>UTC start date</label>
			<name>utc_start_date</name>
			<type>DATE_ISO8601</type>
		</attribute>
		<attribute optional="true">
			<label>UTC end date</label>
			<name>utc_end_date</name>
			<type>DATE_ISO8601</type>
		</attribute>
		<attribute optional="true">
			<label>TAI start date</label>
			<name>tai_start_date</name>
			<type>DATE_ISO8601</type>
		</attribute>
		<attribute optional="true">
			<label>TAI end date</label>
			<name>tai_end_date</name>
			<type>DATE_ISO8601</type>
		</attribute>
		<attribute optional="true">
			<label>Validition flag</label>
			<name>valid</name>
			<type>BOOLEAN</type>
		</attribute>
		<attribute optional="true">
			<label>Validity change date</label>
			<name>validity_change_date</name>
			<type>DATE_ISO8601</type>
        </attribute>
		<attribute optional="true">
			<label>Validity flag</label>
			<name>validity_flag</name>
			<type>BOOLEAN</type>
        </attribute>
	</fragment>
	<!-- JPL SDS -->
	<fragment>
		<name>jpl_sds</name>
		<attribute optional="true">
			<label>JPL SDS transfer start</label>
			<name>tstart</name>
			<type>DATE_ISO8601</type>
        </attribute>
		<attribute optional="true">
			<label>JPL SDS transfer end</label>
			<name>tend</name>
			<type>DATE_ISO8601</type>
        </attribute>
		<attribute optional="true">
			<label>JPL SDS transfer type</label>
			<name>ttype</name>
			<type>STRING</type>
			<restriction>
				<enumeration>
					<value>download</value>
					<value>upload</value>
				</enumeration>
			</restriction>
        </attribute>
	</fragment>
	<!-- JPL GDS -->
	<fragment>
		<name>jpl_gds</name>
		<attribute optional="true">
			<label>JPL GDS transfer start</label>
			<name>tstart</name>
			<type>DATE_ISO8601</type>
        </attribute>
		<attribute optional="true">
			<label>JPL GDS transfer end</label>
			<name>tend</name>
			<type>DATE_ISO8601</type>
        </attribute>
		<attribute optional="true">
			<label>JPL GDS transfer type</label>
			<name>ttype</name>
			<type>STRING</type>
			<restriction>
				<enumeration>
					<value>download</value>
					<value>upload</value>
				</enumeration>
			</restriction>
        </attribute>
	</fragment>
	<!-- SCC -->
	<fragment>
		<name>scc</name>
		<attribute optional="true">
			<label>SCC transfer start</label>
			<name>tstart</name>
			<type>DATE_ISO8601</type>
        </attribute>
		<attribute optional="true">
			<label>SCC transfer end</label>
			<name>tend</name>
			<type>DATE_ISO8601</type>
        </attribute>
		<attribute optional="true">
			<label>SCC transfer type</label>
			<name>ttype</name>
			<type>STRING</type>
			<restriction>
				<enumeration>
					<value>download</value>
					<value>upload</value>
				</enumeration>
			</restriction>
        </attribute>
	</fragment>
	<!-- REGARDS -->
	<fragment>
		<name>regards</name>
		<attribute optional="true">
			<label>REGARDS transfer start</label>
			<name>tstart</name>
			<type>DATE_ISO8601</type>
        </attribute>
		<attribute optional="true">
			<label>REGARDS delivery end</label>
			<name>delivery_end</name>
			<type>DATE_ISO8601</type>
        </attribute>
		<attribute optional="true">
			<label>REGARDS archiving status</label>
			<name>archived</name>
			<type>BOOLEAN</type>
        </attribute>
		<attribute optional="true">
			<label>REGARDS distribution status</label>
			<name>distributed</name>
			<type>BOOLEAN</type>
        </attribute>
		<attribute optional="true">
			<label>REGARDS distribution date</label>
			<name>distribution_date</name>
			<type>DATE_ISO8601</type>
        </attribute>
	</fragment>
	<!-- SWOT -->
	<fragment>
		<name>swot</name>
		<attribute optional="true">
			<label>CRID</label>
			<name>CRID</name>
			<type>STRING</type>
		</attribute>
		<attribute optional="true">
			<label>Product counter</label>
			<name>product_counter</name>
			<type>INTEGER</type>
			<restriction>
				<integer_range>
					<min>1</min>
					<max>99</max>
				</integer_range>
			</restriction>
		</attribute>		
		<attribute optional="true">
			<label>Station</label>
			<name>station</name>
			<type>STRING</type>
			<restriction>
				<enumeration>
					<value>KUX</value>
					<value>IVK</value>
				</enumeration>
			</restriction>
		</attribute>
		<attribute optional="true">
			<label>Day date</label>
			<name>day_date</name>
			<type>DATE_ISO8601</type>
		</attribute>
		<attribute optional="true">
			<label>Cycle</label>
			<name>cycle_number</name>
			<type>INTEGER</type>
		</attribute>
		<attribute optional="true">
			<label>Pass</label>
			<name>pass_number</name>
			<type>INTEGER</type>
		</attribute>
		<attribute optional="true">
			<label>Tile</label>
			<name>tile_number</name>
			<type>INTEGER</type>
		</attribute>
		<attribute optional="true">
			<label>Tile side</label>
			<name>tile_side</name>
			<type>STRING</type>
			<restriction>
				<enumeration>
					<value>Full</value>
					<value>Left</value>
					<value>Right</value>
				</enumeration>
			</restriction>
		</attribute>
		<attribute optional="true">
			<label>L2 LR File id</label>
			<name>file_identifier</name>
			<type>STRING</type>
			<restriction>
				<enumeration>
					<value>Basic</value>
					<value>WindWave</value>
					<value>Expert</value>
					<value>Unsmoothed</value>
				</enumeration>
			</restriction>
		</attribute>
		<attribute optional="true">
			<label>L0A or LOAP APID</label>
			<name>apid</name>
			<type>STRING</type>
		</attribute>
		<attribute optional="true">
			<label>Granule type</label>
			<name>granule_type</name>
			<type>STRING</type>
			<restriction>
				<enumeration>
					<value>Downlink unit</value>
					<value>Cycle</value>
					<value>Half Orbit</value>
					<value>Tile</value>
					<value>Full-swath scene</value>
					<value>Bassin-cycle</value>
					<value>Continent-pass</value>
					<value>Daily</value>
				</enumeration>
			</restriction>
		</attribute>
		<attribute optional="true">
			<label>Continent id</label>
			<name>continent_id</name>
			<type>STRING_ARRAY</type>
		</attribute>
		<attribute optional="true">
			<label>Bassin id</label>
			<name>bassin_id</name>
			<type>STRING</type>
		</attribute>
	</fragment>
	<!-- CORPUS -->
	<fragment>
		<name>corpus</name>
		<attribute optional="true">
			<label>Corpus id</label>
			<name>corpus_id</name>
			<type>INTEGER</type>
		</attribute>
		<attribute optional="true">
			<label>Corpus lot id</label>
			<name>corpus_lot</name>
			<type>STRING</type>
		</attribute>
	</fragment>
</model>