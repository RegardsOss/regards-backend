{
  "name": "frontend-webapp",
  "version": "1.0.0",
  "description": "REGARDS frontend webapp",
  "main": "scripts/index.js",
  "private": true,
  "scripts": {
    "prestart": "cp index.html build/",
    "start": "webpack-dev-server --progress --config webpack.dev.config.js",
    "prebuild": "npm test",
    "build": "webpack --progress --config webpack.dev.config.js",
    "prebuildp": "npm install && npm test",
    "buildp": "webpack --progress --config webpack.prod.config.js",
    "test": "mocha --compilers js:babel-register tests --recursive",
    "test:watch": "npm test -- --watch",
    "presitemap": "webpack --config webpack.sitemap.config.js --progress",
    "sitemap": "node build/sitemap.js"
  },
  "author": "CS-SI",
  "license": "ISC",
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "dependencies": {
    "deep-freeze": "0.0.1",
    "file-loader": "^0.8.5",
    "foundation-sites": "^6.2.3",
    "isomorphic-fetch": "^2.2.1",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.0",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0",
    "scriptjs": "^2.5.8"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel": "^6.5.2",
    "babel-core": "^6.9.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.9.0",
    "chai": "^3.5.0",
    "css-loader": "^0.23.1",
    "enzyme": "^2.3.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "jsdom": "^9.2.1",
    "json-loader": "^0.5.4",
    "mocha": "^2.5.3",
    "nock": "^8.0.0",
    "node-sass": "^3.7.0",
    "postcss-loader": "^0.9.1",
    "react-addons-test-utils": "^15.1.0",
    "redux-mock-store": "^1.1.0",
    "sass-loader": "^3.2.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
