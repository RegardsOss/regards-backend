{
  "name": "frontend-webapp",
  "version": "1.0.0",
  "description": "REGARDS frontend webapp",
  "main": "scripts/index.js",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --progress --config webpack.dev.config.js",
    "prebuild": "npm install && npm test",
    "build": "webpack --progress --config webpack.dev.config.js",
    "postinstall": "typings install",
    "prebuildp": "npm install && npm test",
    "buildp": "webpack --progress --config webpack.prod.config.js",
    "pretest": "tsc",
    "test": "mocha --compilers js:babel-register dist/tests --recursive",
    "test:watch": "npm test --watch",
    "presitemap": "webpack --config webpack.sitemap.config.js --progress",
    "sitemap": "node build/sitemap.js",
    "prejsdoc": "tsc",
    "jsdoc": "jsdoc -c jsdoc.conf.json"
  },
