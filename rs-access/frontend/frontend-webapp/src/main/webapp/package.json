{
  "name": "frontend-webapp",
  "version": "1.0.0",
  "description": "REGARDS frontend webapp",
  "main": "dist/src/main.js",
  "private": true,
  "scripts": {
    "build:development": "npm run prebuild && webpack --progress --config webpack.dev.config.js",
    "build:production": "npm run prebuild && webpack --progress --config webpack.prod.config.js",
    "copy-assets": "cp ./src/common/theme/custom/background.jpg build/src/common/theme/custom/ && cp ./src/common/theme/custom/*.css build/src/common/theme/custom/",
    "jsdoc": "jsdoc -c jsdoc.conf.json",
    "lint": "tslint -c tslint.json 'src/{,**/}*.ts' 'src/{,**/}*.tsx' -e \"**test.tsx{0,1}\"",
    "postinstall": "typings install",
    "prebuild": "npm run preinstall && npm install && npm test",
    "preinstall": "cd web_modules/api && npm link ../models && cd ../.. && npm link web_modules/some-pckg && npm link web_modules/api && npm link web_modules/models",
    "prejsdoc": "tsc",
    "presitemap": "webpack --config webpack.sitemap.config.js --progress",
    "pretest": "(npm run lint --silent || true)",
    "sitemap": "node build/sitemap.js",
    "start": "NODE_ENV=development node  --max_old_space_size=4096 --optimize_for_size --max_executable_size=4096 --stack_size=4096 ./node_modules/.bin/webpack-dev-server --progress --hot --config webpack.dev.config.js",
    "test": "npm run test:mocha --silent",
    "test:coverage": "NODE_ENV=test nyc npm run test:mocha",
    "test:mocha": "mocha-webpack --webpack-config webpack.test.config.js \"src/**/*.test.\\?(ts|tsx)\" --inline-diffs --recursive --reporter mochawesome --reporter-options reportDir='./coverage/mocha',reportName='Regards Frontend tests'",
    "test:watch": "npm test -- --watch",
    "tsc:watch": "tsc -w",
    "typings": "typings install --global --save"
  },
  "author": "CS-SI",
  "license": "ISC",
  "babel": {
    "presets": [
      "es2015",
      "react"
    ],
    "env": {
      "development": {
        "plugins": [
          "react-hot-loader/babel"
        ]
      },
      "test": {
        "plugins": [
          "istanbul"
        ]
      }
    }
  },
  "nyc": {
    "include": [
      "dist/src/**/*.js"
    ],
    "exclude": [
      "dist/src/**/messages.*.i18n.js"
    ],
    "require": [
      "babel-core/register",
      "source-map-support/register"
    ],
    "reporter": [
      "json",
      "html",
      "text"
    ],
    "all": true
  },
  "dependencies": {
    "flux-standard-action": "^0.6.1",
    "immutable": "^3.8.1",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.13.1",
    "material-ui": "^0.15.4",
    "normalizr": "^2.1.0",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-intl": "^2.1.3",
    "react-redux": "^4.4.5",
    "react-resizable": "^1.4.2",
    "react-router": "^2.4.0",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.5.2",
    "redux-api-middleware": "^1.0.2",
    "redux-form": "^5.2.5",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0",
    "scriptjs": "^2.5.8",
    "sockjs-client": "^1.1.1",
    "stompjs": "^2.3.3",
    "@regardsoss/api": "0.0.1",
    "@regardsoss/models": "0.0.1",
    "@regardsoss/some-pckg": "0.0.1"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.9.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.9.0",
    "chai": "^3.5.0",
    "clean-webpack-plugin": "^0.1.9",
    "css-loader": "^0.23.1",
    "deep-freeze": "0.0.1",
    "enzyme": "^2.3.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "istanbul": "^1.1.0-alpha.1",
    "jsdoc": "^3.4.0",
    "json-loader": "^0.5.4",
    "mocha": "^2.5.3",
    "mocha-loader": "^0.7.1",
    "mocha-webpack": "^0.6.0",
    "mochawesome": "^1.5.1",
    "nock": "^8.0.0",
    "nyc": "^7.1.0",
    "postcss-loader": "^0.9.1",
    "react-addons-test-utils": "^15.1.0",
    "react-hot-loader": "^3.0.0-beta.3",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "redux-mock-store": "^1.2.0",
    "sinon": "^1.17.4",
    "source-map-loader": "^0.1.5",
    "source-map-support": "^0.4.2",
    "style-loader": "^0.13.1",
    "ts-loader": "^0.8.2",
    "tslint": "^3.13.0",
    "tslint-eslint-rules": "^1.3.0",
    "typescript": "^2.0.0-beta",
    "typings": "^1.3.2",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1",
    "webpack-merge": "^0.14.1",
    "webpack-node-externals": "^1.3.3"
  },
  "engines": {
    "node": ">=0.12.0",
    "npm": "^3.10.7"
  }
}
