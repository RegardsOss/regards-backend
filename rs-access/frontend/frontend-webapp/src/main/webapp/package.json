{
  "name": "frontend-webapp",
  "version": "1.0.0",
  "description": "REGARDS frontend webapp",
  "main": "scripts/index.js",
  "private": true,
  "scripts": {
    "start": "webpack-dev-server --progress --config webpack.dev.config.js",
    "prebuild": "npm install && npm test",
    "build": "webpack --progress --config webpack.dev.config.js",
    "postinstall": "typings install",
    "typings": "typings install --global --save",
    "prebuildp": "npm install && npm test",
    "buildp": "webpack --progress --config webpack.prod.config.js",
    "copy-assets": "cp ./scripts/common/theme/custom/background.jpg dist/scripts/common/theme/custom/",
    "pretest": "tsc && (npm run lint --silent || true) && npm run copy-assets",
    "test": "mocha --require ./tests/ImportHelper.mock.test.js --require source-map-support/register --compilers js:babel-register dist/tests --inline-diffs --recursive",
    "test:watch": "npm test -- --watch",
    "test-cov": "nyc npm test",
    "presitemap": "webpack --config webpack.sitemap.config.js --progress",
    "sitemap": "node build/sitemap.js",
    "prejsdoc": "tsc",
    "tsc:watch": "tsc -w",
    "jsdoc": "jsdoc -c jsdoc.conf.json",
    "lint": "tslint -c tslint.json 'scripts/{,**/}*.ts' 'scripts/{,**/}*.tsx'"
  },
  "author": "CS-SI",
  "license": "ISC",
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "nyc": {
    "include": [
      "dist/scripts/**/*.js"
    ],
    "require": [
      "babel-core/register",
      "source-map-support/register"
    ],
    "reporter": [
      "json",
      "html"
    ],
    "all": true
  },
  "dependencies": {
    "flux-standard-action": "^0.6.1",
    "immutable": "^3.8.1",
    "isomorphic-fetch": "^2.2.1",
    "lodash": "^4.13.1",
    "material-ui": "^0.15.3",
    "normalizr": "^2.1.0",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-grid-layout": "^0.13.0",
    "react-intl": "^2.1.3",
    "react-redux": "^4.4.5",
    "react-resizable": "^1.4.2",
    "react-router": "^2.4.0",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.5.2",
    "redux-api-middleware": "^1.0.2",
    "redux-form": "^5.2.5",
    "redux-logger": "^2.6.1",
    "redux-thunk": "^2.1.0",
    "scriptjs": "^2.5.8",
    "sockjs-client": "^1.1.1",
    "stompjs": "^2.3.3"
  },
  "devDependencies": {
    "autoprefixer": "^6.3.6",
    "babel-cli": "^6.11.4",
    "babel-core": "^6.9.0",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-register": "^6.9.0",
    "chai": "^3.5.0",
    "clean-webpack-plugin": "^0.1.9",
    "css-loader": "^0.23.1",
    "deep-freeze": "0.0.1",
    "enzyme": "^2.3.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "istanbul": "^1.1.0-alpha.1",
    "jsdoc": "^3.4.0",
    "json-loader": "^0.5.4",
    "mocha": "^3.0.0-2",
    "nock": "^8.0.0",
    "nyc": "^7.1.0",
    "postcss-loader": "^0.9.1",
    "react-addons-test-utils": "^15.1.0",
    "redux-devtools": "^3.3.1",
    "redux-devtools-dock-monitor": "^1.1.1",
    "redux-devtools-log-monitor": "^1.0.11",
    "redux-mock-store": "^1.1.2",
    "sinon": "^1.17.4",
    "source-map-loader": "^0.1.5",
    "source-map-support": "^0.4.2",
    "style-loader": "^0.13.1",
    "ts-loader": "^0.8.2",
    "tslint": "^3.13.0",
    "tslint-eslint-rules": "^1.3.0",
    "typescript": "^2.0.0-beta",
    "typings": "^1.3.2",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "engines": {
    "node": ">=0.12.0"
  }
}
